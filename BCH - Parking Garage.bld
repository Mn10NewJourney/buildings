#Set textures
<Function FloorTex>
	Set 5 = Road
	Set 5A = PGLobbyFloor #Elevator lobby floor
	if[%floor% > 2] Set 5A = PGLobbyFloor2 #Elevator lobby floor
<EndFunction>

<Function WallTex>
	Set 6 = ParkingWall
	#if[%floor% < 0] Set 6 = ExtFacade2
	if[%floor% > 5] Set 6 = ExtFacade

	Set 6Ramp = ParkingWall #Walls around ramp
	Set 6A = PGLobbyWall #Elevator lobby wall
	if[%floor% > 2] Set 6A = PGLobbyWall2 #Elevator lobby wall
	Set 6B = ParkingWall2 #Support columns
<EndFunction>

<Function CeilingTex>
	Set 7 = ParkingCeiling
	Set 7A = PGLobbyCeiling #Elevator lobby ceiling
<EndFunction>

