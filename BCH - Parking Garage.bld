#Exterior & interior
#All levels' detailing
<Floors -1 to 6>
	Set 1 = 64 #Building Dimension X
	Set 2 = 120 #Building Dimension Z
	FloorTex() #Set floor Texture
	WallTex() #Set wall texture
	CeilingTex() #Ceiling Texture

	#Floors
	Set HalfFlrHeight = %interfloorheight% + (%fullheight% / 2)
	Set FullFlrHeight = %fullheight% + Floor(%floor% + 1).InterfloorHeight

	FloorOrientation = top
	DrawWalls = false, true, false, false, false, false
	AddFloor EastFloor1, %5%, 0.5, 0 + 20, -%2%, %1%, %2%, %interfloorheight%, %interfloorheight%, 0, 0, true
	if[%floor% > -1] AddFloor EastFloor2, %5%, 0.5, 0, -%2% + 30, 0 + 20, %2% - 30, %interfloorheight%, %interfloorheight%, 0, 0, true
	if[%floor% = -1] AddFloor EastFloor2, %5%, 0.5, 0, -%2% + 15, 0 + 20, %2% - 30, %interfloorheight%, %interfloorheight%, 0, 0, true
	if[%floor% = -1] AddFloor EastFloor2A, %5%, 0.5, 0, %2% - 15, 0 + 20, %2%, %interfloorheight%, %interfloorheight%, 0, 0, true
	if[%floor% < 6] AddFloor WestFloor1, %5%, 0.5, -%1%, -%2%, 0 - 20, %2%, %HalfFlrHeight%, %HalfFlrHeight%, 0, 0, true
	if[%floor% < 6] AddFloor WestFloor2, %5%, 0.5, 0 - 20, -%2% + 30, , %2% - 30, %HalfFlrHeight%, %HalfFlrHeight%, 0, 0, true

		#Elevator lobby floor
		CutAll %1% - 20, %interfloorheight% - 0.1, -%2%, %1%, %interfloorheight% + 0.1, -%2% + 40, false, true #South
		CutAll %1% - 20, %interfloorheight% - 0.1, %2% - 40 - 9, %1%, %interfloorheight% + 0.1, %2%, false, true #North
		AddFloor SouthLobbyFloor, %5A%, 0.5, %1% - 20, -%2%, %1%, -%2% + 40, %interfloorheight%, %interfloorheight%, 0, 0, true
		AddFloor NorthLobbyFloor, %5A%, 0.5, %1% - 20, %2% - 40 - 9, %1%, %2%, %interfloorheight%, %interfloorheight%, 0, 0, true

	#Ceilings
	Set ParkingHeight = %fullheight% + (Floor(%floor% + 1).InterfloorHeight - 0.5)
	Set HalfCeiHeight = %ParkingHeight% + (%fullheight% / 2)

	FloorOrientation = bottom
