#Freight elevator
<Function SE_HallTx>
	#Shaft doors
	Set SDoorTexture = ElevDoorsB
	if[%floor% > 0 & %floor% < 10] Set SDoorTexture = ElevDoorsA
	if[%floor% = 10] Set SDoorTexture = ElevDoorsC

