#Create hall stations (Freight)
<Floors 0 to 10>
	#Shaft Doors
	SE_HallTx() #Set hall textures for freight elevator

		WallOrientation = right
		Set InitialDoorPos = -3.84 - (0.15 * 3) - 0.1

		AddShaftDoorComponent 5, 1, 3Speed_Door1, %SDoorTexture%, %SDoorTexture%, 0.15, right, 0.4, 0.3, %InitialDoorPos% - (0.15 * 2), 2.5 - (2 * 1), %InitialDoorPos% - (0.15 * 2), 2.5, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent 5, 1, 3Speed_Door2, %SDoorTexture%, %SDoorTexture%, 0.15, right, 0.8, 0.6, %InitialDoorPos% - (0.15 * 1), 2.5 - (2 * 2), %InitialDoorPos% - (0.15 * 1), 2.5 - (2 * 1), 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent 5, 1, 3Speed_Door3, %SDoorTexture%2, %SDoorTexture%2, 0.15, right, 1.2, 0.9, %InitialDoorPos%, 2.5 - (2 * 3), %InitialDoorPos%, 2.5 - (2 * 2), 7, 0, 0, 0, 0, 0
		FinishShaftDoor 5, 1, %AutoFiller%, false

		#Gap fillers
		WallOrientation = left
		DrawWalls = true, true, true, true, true, true
		TextureOverride ElevExt, ElevExt, ElevExt, %SDoorTrimTexture%, ElevExt, ElevExt
		AddShaftWall 3, E5GapFiller, ElevExt, 0.15 * 3, -5.17 + 0.33, -4.5 - 0.05, -5.17 + 0.33, -4.5, 7, 7, %interfloorheight%, %interfloorheight%, 0, 0

	#Door track
	AddDoorTrack(5, 3, -5.17 + 0.33, -5.17 + 0.33 + 0.45, -4.5, -4.5 + 6 + 2, ElevDoorTrackX)

	#Top fillers
	AddTopFiller(5A, 3, -5.17 + 0.33, -5.17 + 0.33 + 0.15, -4.5, -4.5 + 4, 7, %SDoorTrimTexture%)
	AddTopFiller(5B, 3, -5.17 + 0.33 + 0.15, -5.17 + 0.33 + 0.3, -4.5, -4.5 + 2, 7, %SDoorTrimTexture%)

	#****************************************#

	#Door trims/fillers
	#WallTex() #Set filler textures to match with shaft walls
	AddDeco4(%SDoorTrimTexture%, %SDoorTrimTexture_Top%, left, true, false, floor, 0.05, 0.2, 9, -17.5, 6, 7, %SSideDecoH%, %STopDecoH%, 0, true, %SDoorTrimTexture%, 0.66)

		#WallOrientation = left
		#if[%floor% = 10] AddFillerWalls %6%, 0.66, 9, -17.5, 6, 7, 0, false, 0, 0, false

	#****************************************#

	#Call buttons
	Set ButtonPosition = -14.5 + 1.25

	CallButtonElevators = 5
	CreateCallButtons %SBPanelTexture%, %ButtonPrefix%Up, %ButtonPrefix%UpLit, %ButtonPrefix%Down, %ButtonPrefix%DownLit, 9 - 0.001, %ButtonPosition%, 3.5, left, 0.5, 1, true, 0, 0

	#****************************************#

	#Indicator sets
	Set Offset = 7.5
	if[%floor% = 0] Set Offset = 7.75

		#Set textures
		ACMEInd_LanternTex(VRLanternUp, VRLanternUpLit, VRLanternDown, VRLanternDownLit, %SLPanelTexture%, OutsideInd_Button)

		#Indicator creation
		ACMELobbyInds(left, 5, 9, -17.5, %Offset%, 0, 10)
<EndFloors>


