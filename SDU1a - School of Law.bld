	Set LPanelTexture = %BPanelTexture%

		#Lateral offset for lantern panels
		Set ExtraLateralOffset = 0
		if[%floor% = 0] Set ExtraLateralOffset = 0.05
<EndFunction>

#Create hall stations for passenger elevators
<Floors 0 to 9>
	#Shaft Doors
	ElevHallTex() #Set shaft door and panel textures

		#Elevators 1-2
		Set StartNo = 1
		WallOrientation = right
		AddShaftDoorComponent %StartNo%, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, left, 0.6, -2, -2.8, 0, -2.8, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent %StartNo%, 1, Right, (%DoorTexture%)2, (%DoorTexture%)2, 0.2, right, 0.6, 0, -2.8, 2, -2.8, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo%, 1, %AutoFiller%, false
	
		WallOrientation = right
		AddShaftDoorComponent %StartNo% + 1, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, left, 0.6, -2, -2.8, 0, -2.8, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent %StartNo% + 1, 1, Right, (%DoorTexture%)2, (%DoorTexture%)2, 0.2, right, 0.6, 0, -2.8, 2, -2.8, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo% + 1, 1, %AutoFiller%, false
	
		#Elevators 3-4
		Set StartNo = 3
		WallOrientation = left
		AddShaftDoorComponent %StartNo%, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, right, 0.6, 0, 2.8, 2, 2.8, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent %StartNo%, 1, Right, (%DoorTexture%)2, (%DoorTexture%)2, 0.2, left, 0.6, -2, 2.8, 0, 2.8, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo%, 1, %AutoFiller%, false
	
		WallOrientation = left
		AddShaftDoorComponent %StartNo% + 1, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, right, 0.6, 0, 2.8, 2, 2.8, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent %StartNo% + 1, 1, Right, (%DoorTexture%)2, (%DoorTexture%)2, 0.2, left, 0.6, -2, 2.8, 0, 2.8, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo% + 1, 1, %AutoFiller%, false

	#Door tracks
	AddDoorTrack(1, 1, -5 - 4, -5 + 4, -4.67 + 0.33, -4.67 + 0.33 + 0.2, ElevDoorTrackZ)
	AddDoorTrack(2, 1, 5 - 4, 5 + 4, -4.67 + 0.33, -4.67 + 0.33 + 0.2, ElevDoorTrackZ)
	AddDoorTrack(3, 2, -5 - 4, -5 + 4, 4.67 - 0.33 - 0.2, 4.67 - 0.33, ElevDoorTrackZ)
	AddDoorTrack(4, 2, 5 - 4, 5 + 4, 4.67 - 0.33 - 0.2, 4.67 - 0.33, ElevDoorTrackZ)

	#****************************************#

	#Door trims/fillers
	#WallTex() #Set filler textures to match with shaft walls

		#Elevators 1-2
		AddDeco4(%DoorTrimTexture%, %DoorTrimTexture_Top%, front, true, false, floor, 0.05, 0.2, 5, 10, 4, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66)
		AddDeco4(%DoorTrimTexture%, %DoorTrimTexture_Top%, front, true, false, floor, 0.05, 0.2, 15, 10, 4, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66)

		#Elevators 3-4
		AddDeco4(%DoorTrimTexture%, %DoorTrimTexture_Top%, back, true, false, floor, 0.05, 0.2, 5, 0, 4, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66)
		AddDeco4(%DoorTrimTexture%, %DoorTrimTexture_Top%, back, true, false, floor, 0.05, 0.2, 15, 0, 4, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66)

	#****************************************#

	#Call buttons
	CallButtonElevators = 1, 2, 3, 4
	CreateCallButtons %BPanelTexture%, %ButtonPrefix%Up, %ButtonPrefix%UpLit, %ButtonPrefix%Down, %ButtonPrefix%DownLit, 10, 10 - 0.001, 3.5, front, 0.5, 1, true, 0, 0
	CreateCallButtons %BPanelTexture%, %ButtonPrefix%Up, %ButtonPrefix%UpLit, %ButtonPrefix%Down, %ButtonPrefix%DownLit, 10, 0 + 0.001, 3.5, back, 0.5, 1, true, 0, 0

		#Signs
		DrawWalls = true, false, false, false, false, false
		AddWall E1-4HallSign1, ElevHallSign, 0, 10 - 0.25, 10 - 0.001, 10 + 0.25, 10 - 0.001, 0.75, 0.75, 2.6, 2.6, 0, 0, false
		DrawWalls = false, true, false, false, false, false
		AddWall E1-4HallSign2, ElevHallSign, 0, 10 - 0.25, 0 + 0.001, 10 + 0.25, 0 + 0.001, 0.75, 0.75, 2.6, 2.6, 0, 0, false

	#****************************************#

	#Indicator sets
	Set Offset = 7.5
	if[%floor% = 0] Set Offset = 8

		#Set textures
		ACMEInd_LanternTex(LanternUp, LanternUpLit, LanternDown, LanternDownLit, %LPanelTexture%, OutsideInd_Button)

		#Indicator creation
		if[%floor% = 0] ACMELobbyInds(front, 1, 5, 10 - %ExtraLateralOffset%, %Offset%, 0, 9)
		if[%floor% = 0] ACMELobbyInds(front, 2, 15, 10 - %ExtraLateralOffset%, %Offset%, 0, 9)
		if[%floor% = 0] ACMELobbyInds(back, 3, 5, 0 + %ExtraLateralOffset%, %Offset%, 0, 9)
		if[%floor% = 0] ACMELobbyInds(back, 4, 15, 0 + %ExtraLateralOffset%, %Offset%, 0, 9)

		if[%floor% ! 0] ACMELobbyLanterns(front, 1, 5, 10 - %ExtraLateralOffset%, %Offset%, 0, 9)
		if[%floor% ! 0] ACMELobbyLanterns(front, 2, 15, 10 - %ExtraLateralOffset%, %Offset%, 0, 9)
		if[%floor% ! 0] ACMELobbyLanterns(back, 3, 5, 0 + %ExtraLateralOffset%, %Offset%, 0, 9)
		if[%floor% ! 0] ACMELobbyLanterns(back, 4, 15, 0 + %ExtraLateralOffset%, %Offset%, 0, 9)
<EndFloors>

