	#External walls
	WallTex()
	CreateShaftWall(%6%, %6%, %6%, %6Hall%, %6%, %6%, 0.5, %21%, %22%, %23% + 0.5, %24% + 0.5, %25%, %height%, 0, true, false, true, true, false, false, false)

