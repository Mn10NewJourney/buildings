	if[%floor% > 2 & %floor% < 16] Set SignOffset = %height% - 0.9
	AddBoxSign(Stair%25%_SignA2, StainlessSteel, ExitSignLeft, StainlessSteel, 0.1, false, true, true, %21% + %23% - 2, -52 + 0.9, 1.8, 0.9, %SignOffset%, false)
