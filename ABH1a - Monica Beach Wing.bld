	AddStairsWall %25%, Stair%25%Center, StairsWall2, 1, 0, -%24% + 5, 0, -%24% + 5 + %NumSteps%, %fullheight%, %fullheight%, -%interfloorheight%, -%interfloorheight%, 0, 0
	DrawWalls = true, true, true, true, true, false
	if[%floor% = 3] AddStairsWall %25%, Stair%25%TransitionWall, StairsWall2, 1, 0, -%24% + 5 + %NumSteps%, 0, -%24% + 5 + %NumStepsAlt%, %interfloorheight% + 3, %interfloorheight% + 3, -%interfloorheight%, -%interfloorheight%, 0, 0

