#Functions
#Elevator interior function for this Wing
<Function ElevInt_Monica>
	Set FloorTex = %param1%
	Set CeilingTex = %param2%
	Set WallTex_X = %param3%
	Set WallTex_Z = %param4%
	Set WidthX = %param5%
	Set LengthZ = %param6%
	Set Direction = %param7%
	Set ElevHeight = %param8%
	Set DoorWidth = %param9%

	Set DoorWidthHalf = %DoorWidth% / 2
	Set ElevWidth = %WidthX% / 2
	Set ElevLength = %LengthZ% / 2

	#Floor
	FloorOrientation = top
	DrawWalls = true, true, true, true, true, true
	TextureOverride ElevExt2, %FloorTex%, ElevExt2, ElevExt2, ElevExt2, ElevExt2
	AddFloor Floor, %FloorTex%, 1.5, -%ElevWidth%, -%ElevLength%, %ElevWidth%, %ElevLength%, 0, 0, 0, 0

	#Ceiling
	Set ExtTexA = ElevExt2
	if[%Direction% = Right | %Direction% = right] Set ExtTexA = ElevExt3
	Set ExtTexB = ElevExt2
	if[%Direction% = Left | %Direction% = left] Set ExtTexB = ElevExt3

	FloorOrientation = bottom
	TextureOverride %CeilingTex%, ElevExt2, %ExtTexA%, %ExtTexB%, ElevExt2, ElevExt2
	AddFloor Ceiling, %CeilingTex%, 1.5, -%ElevWidth%, -%ElevLength%, %ElevWidth%, %ElevLength%, %ElevHeight%, %ElevHeight%, 0, 0

	#Exterior walls
	WallOrientation = left
	DrawWalls = true, false, false, false, false, false
	if[%Direction% ! Front & %Direction% ! front] AddWall FrontExt, ElevExt2, 0.2, -%ElevWidth%, -%ElevLength%, %ElevWidth%, -%ElevLength%, %ElevHeight%, %ElevHeight%, 0, 0, 0, 0

	WallOrientation = right
	DrawWalls = false, true, false, false, false, false
	if[%Direction% ! Back & %Direction% ! back] AddWall BackExt, ElevExt2, 0.2, -%ElevWidth%, %ElevLength%, %ElevWidth%, %ElevLength%, %ElevHeight%, %ElevHeight%, 0, 0, 0, 0

	#Walls
	WallOrientation = left
	DrawWalls = false, true, false, false, false, false
	TextureOverride ElevExt2, %WallTex_Z%, ElevExt2, ElevExt2, ElevExt2, ElevExt2
	if[%Direction% ! Front & %Direction% ! front] AddWall Front, %WallTex_Z%, 0.2, -%ElevWidth% + 0.2, -%ElevLength%, %ElevWidth% - 0.2, -%ElevLength%, %ElevHeight%, %ElevHeight%, 0, 0, 0, 0
	DrawWalls = true, true, false, false, false, false
	#TextureOverride ElevExt2, %WallTex_X%, ElevExt2, ElevExt2, ElevExt2, ElevExt2
	#if[%Direction% ! Left & %Direction% ! left] AddWall Left, %WallTex_X%, 0.2, -%ElevWidth%, -%ElevLength%, -%ElevWidth%, %ElevLength%, %ElevHeight%, %ElevHeight%, 0, 0, 0, 0

	WallOrientation = right
	DrawWalls = true, true, false, false, false, false
	#TextureOverride %WallTex_X%, ElevExt2, ElevExt2, ElevExt2, ElevExt2, ElevExt2
	#if[%Direction% ! Right & %Direction% ! right] AddWall Right, %WallTex_X%, 0.2, %ElevWidth%, -%ElevLength%, %ElevWidth%, %ElevLength%, %ElevHeight%, %ElevHeight%, 0, 0, 0, 0
	DrawWalls = true, false, false, false, false, false
	TextureOverride %WallTex_Z%, ElevExt2, ElevExt2, ElevExt2, ElevExt2, ElevExt2
	if[%Direction% ! Back & %Direction% ! back] AddWall Back, %WallTex_Z%, 0.2, -%ElevWidth% + 0.2, %ElevLength%, %ElevWidth% - 0.2, %ElevLength%, %ElevHeight%, %ElevHeight%, 0, 0, 0, 0

	#Panels (right-facing)
	WallOrientation = right
	DrawWalls = true, true, false, true, false, false
	TextureOverride %Prefix%ElevPanelMain, ElevExt2, ElevExt2, %Prefix%ElevPanelMain, ElevExt2, ElevExt2
	if[%Direction% = Right | %Direction% = right] AddWall SidePanelA, ElevPanelMain, 0.2, %ElevWidth%, -%ElevLength%, %ElevWidth%, -%DoorWidthHalf%, %ElevHeight%, %ElevHeight%, 0, 0, 0, 0
	DrawWalls = true, true, true, false, false, false
	TextureOverride %Prefix%ElevPanelMain, ElevExt2, %Prefix%ElevPanelMain, ElevExt2, ElevExt2, ElevExt2
	if[%Direction% = Right | %Direction% = right] AddWall SidePanelB, ElevPanelMain, 0.2, %ElevWidth%, %DoorWidthHalf%, %ElevWidth%, %ElevLength%, %ElevHeight%, %ElevHeight%, 0, 0, 0, 0
	DrawWalls = true, true, false, false, false, true
	TextureOverride %Prefix%ElevPanelTop, ElevExt2, ElevExt2, ElevExt2, ElevExt2, %Prefix%ElevPanelTop
	if[%Direction% = Right | %Direction% = right] AddWall TopPNL, ElevPanelTop, 0.2, %ElevWidth%, -%DoorWidthHalf%, %ElevWidth%, %DoorWidthHalf%, %ElevHeight% - 7, %ElevHeight% - 7, 7, 7, 0, 0

	#Panels (left-facing)
	WallOrientation = left
	DrawWalls = true, true, false, true, false, false
	TextureOverride ElevExt2, %Prefix%ElevPanelMain, ElevExt2, %Prefix%ElevPanelMain, ElevExt2, ElevExt2
	if[%Direction% = Left | %Direction% = left] AddWall SidePanelA, ElevPanelMain, 0.2, -%ElevWidth%, -%ElevLength%, -%ElevWidth%, -%DoorWidthHalf%, %ElevHeight%, %ElevHeight%, 0, 0, 0, 0
	DrawWalls = true, true, true, false, false, false
	TextureOverride ElevExt2, %Prefix%ElevPanelMain, %Prefix%ElevPanelMain, ElevExt2, ElevExt2, ElevExt2
	if[%Direction% = Left | %Direction% = left] AddWall SidePanelB, ElevPanelMain, 0.2, -%ElevWidth%, %DoorWidthHalf%, -%ElevWidth%, %ElevLength%, %ElevHeight%, %ElevHeight%, 0, 0, 0, 0
	DrawWalls = true, true, false, false, false, true
	TextureOverride ElevExt2, %Prefix%ElevPanelTop, ElevExt2, ElevExt2, ElevExt2, %Prefix%ElevPanelTop
	if[%Direction% = Left | %Direction% = left] AddWall TopPNL, ElevPanelTop, 0.2, -%ElevWidth%, -%DoorWidthHalf%, -%ElevWidth%, %DoorWidthHalf%, %ElevHeight% - 7, %ElevHeight% - 7, 7, 7, 0, 0
<EndFunction>

