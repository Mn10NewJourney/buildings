#Set textures
<Function FloorTex>
	if[%floor% < 3] Set 5 = LobbyFloor #Floor Texture
	if[%floor% > 2 & %floor% < 16] Set 5 = RoomFloor #Floor Texture
	if[%floor% = 16] Set 5 = SummitFloor #Floor Texture
	if[%floor% = 17] Set 5 = ServiceFloor #Floor Texture
	if[%floor% = 18] Set 5 = RoofFloor #Floor Texture

	Set 5B = RoofFloor #Roof Texture
<EndFunction>

<Function WallTex>
	if[%floor% < 3] Set 6 = LobbyWall #Wall Texture
	if[%floor% > 2 & %floor% < 16] Set 6 = RoomWall
	if[%floor% = 16] Set 6 = SummitWall #Wall Texture
	if[%floor% > 16] Set 6 = ServiceWall #Wall Texture
	
	#Override textures on guestroom levels
		#Passenger elevator lobby
		Set 6Lobby = %6%
		if[%floor% > 2 & %floor% < 16] Set 6Lobby = RoomLobbyWall

		#Atrium railings
		Set 6Rail = %6%
		if[%floor% > 2 & %floor% < 16] Set 6Rail = RoomLobbyRailing

		#Service elevator lobby
		Set 6Service = %6%
		if[%floor% > 2 & %floor% < 16] Set 6Service = RoomServiceWall

		#Hallway walls
		Set 6Hall = %6%
		if[%floor% > 2 & %floor% < 16] Set 6Hall = RoomHallwayWall

	#Override wall texture for support columns
	Set 6A = %6%

	#Override wall texture for atrium walls
	Set 6B = %6%
	if[%floor% > 2 & %floor% < 16] Set 6B = %6Rail%
	if[%floor% > 16] Set 6B = SummitWall
<EndFunction>

<Function CeilingTex>
	if[%floor% < 3] Set 7 = LobbyCeiling #Ceiling Texture
	if[%floor% > 2 & %floor% < 16] Set 7 = RoomCeiling
	if[%floor% = 16] Set 7 = SummitCeiling #Ceiling Texture
	if[%floor% > 16] Set 7 = ServiceCeiling #Ceiling Texture

	#Override for atrium ceiling
	Set 7B = %7%
	if[%floor% > 16] Set 7B = SummitCeiling
<EndFunction>

