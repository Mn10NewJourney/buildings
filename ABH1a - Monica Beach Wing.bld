<Function ElevHallTex>
	#Shaft doors
	if[%floor% = 1] Set DoorTexture = LobbyElevDoors
	if[%floor% = 0 | %floor% = 2] Set DoorTexture = UpperLobbyElevDoors
	if[%floor% > 2 & %floor% < 16] Set DoorTexture = GuestroomElevDoors
	if[%floor% = 16] Set DoorTexture = SummitElevDoors
	
	#Door trims
		#Sides
		Set DoorTrimTexture = %DoorTexture%_Trim
		if[%floor% = 0 | %floor% = 2] Set DoorTrimTexture = LobbyElevDoors_Trim

		#Top
		Set DoorTrimTexture_Top = %DoorTrimTexture%
		if[%floor% = 16] Set DoorTrimTexture_Top = SummitElevDoors_TrimTop

	#Deco dimensions
		Set SideDecoH = 7.2
		Set TopDecoH = 0.2
		if[%floor% < 3 | %floor% = 16] Set SideDecoH = %height%
		if[%floor% < 3 | %floor% = 16] Set TopDecoH = %height% - 7

	#Call button panels
	Set BPanelTexture = ElevExtPanels
	if[%floor% > 2 & %floor% < 16] Set BPanelTexture = ElevExtPanels2
	if[%floor% = 16] Set BPanelTexture = ElevExtPanels

	#Lantern panels
	Set LPanelTexture = %BPanelTexture%
	if[%floor% = 16] Set LPanelTexture = ElevExtPanels
<EndFunction>

