	if[%floor% < 3 | %floor% > 15] Set NumSteps = 12 #Total: 24 steps
	if[%floor% > 2 & %floor% < 16] Set NumSteps = 10 #Total: 20 steps

	#Extra sets at transition points
	Set NumStepsAlt = %NumSteps%
