	CreateCallButtons %BPanelTexture%, %ButtonPrefix%Up, %ButtonPrefix%UpLit, %ButtonPrefix%Down, %ButtonPrefix%DownLit, 5 - 0.001, 5, 3.5, left, 0.5, 1, true, 0, 0

		#Signs
		DrawWalls = false, true, false, false, false, false
		#AddWall E1-4HallSign1, ElevHallSign, 0, -5 + 0.001, 5 - 0.375, -5 + 0.001, 5 + 0.375, 1, 1, 2.4, 2.4, 0, 0, false
		AddWall E1-4FloorSign1, FlrSign%floorid%, 0, -5 + 0.001, 5 - 0.5, -5 + 0.001, 5 + 0.5, 1, 1, 5.5, 5.5, 0, 0, false
		DrawWalls = true, false, false, false, false, false
		#AddWall E1-4HallSign2, ElevHallSign, 0, 5 - 0.001, 5 - 0.375, 5 - 0.001, 5 + 0.375, 1, 1, 2.4, 2.4, 0, 0, false
		AddWall E1-4FloorSign2, FlrSign%floorid%, 0, 5 - 0.001, 5 - 0.5, 5 - 0.001, 5 + 0.5, 1, 1, 5.5, 5.5, 0, 0, false

	#****************************************#

	#Indicator sets
	Set Offset = 7.75
	#if[%floor% > 0] Set Offset = 7.5

		#Set textures
		ACMEInd_LanternTex(LanternUp, LanternUpLit, LanternDown, LanternDownLit, %LPanelTexture%, OutsideInd_Button)

		#Indicator creation
		if[%floor% = 0] ACMELobbyInds(right, 1, -5 + %ExtraLateralOffset%, 10, %Offset%, 0, 7)
		if[%floor% = 0] ACMELobbyInds(right, 2, -5 + %ExtraLateralOffset%, 0, %Offset%, 0, 7)
		if[%floor% = 0] ACMELobbyInds(left, 3, 5 - %ExtraLateralOffset%, 10, %Offset%, 0, 7)
		if[%floor% = 0] ACMELobbyInds(left, 4, 5 - %ExtraLateralOffset%, 0, %Offset%, 0, 7)

		if[%floor% ! 0] ACMELobbyLanterns(right, 1, -5 + %ExtraLateralOffset%, 10, %Offset%, 0, 7)
		if[%floor% ! 0] ACMELobbyLanterns(right, 2, -5 + %ExtraLateralOffset%, 0, %Offset%, 0, 7)
		if[%floor% ! 0] ACMELobbyLanterns(left, 3, 5 - %ExtraLateralOffset%, 10, %Offset%, 0, 7)
		if[%floor% ! 0] ACMELobbyLanterns(left, 4, 5 - %ExtraLateralOffset%, 0, %Offset%, 0, 7)
