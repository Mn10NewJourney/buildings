#Set textures
<Function FloorTex>
	Set 5 = LobbyFloor
	if[%floor% = 1] Set 5 = HangoutFloor
	if[%floor% > 1 & %floor% < 6] Set 5 = ClassroomFloor
	if[%floor% > 5] Set 5 = UpperClassroomFloor
	Set 5B = RoofFloor #Roof

		#Override for elevator lobby
		Set 5Lobby = %5%
		if[%floor% = 1] Set 5Lobby = HangoutLobbyFloor
		if[%floor% > 1 & %floor% < 6] Set 5Lobby = ClassroomLobbyFloor
		if[%floor% > 5] Set 5Lobby = UpperClassroomLobbyFloor
<EndFunction>

<Function WallTex>
	Set 6 = LobbyWall
	if[%floor% = 1] Set 6 = HangoutWall
	if[%floor% > 1 & %floor% < 6] Set 6 = ClassroomWall
	if[%floor% > 5] Set 6 = UpperClassroomWall
<EndFunction>

<Function CeilingTex>
	Set 7 = LobbyCeiling
	if[%floor% = 1] Set 7 = HangoutCeiling
	if[%floor% > 1 & %floor% < 6] Set 7 = ClassroomCeiling
	if[%floor% > 5] Set 7 = UpperClassroomCeiling
<EndFunction>

