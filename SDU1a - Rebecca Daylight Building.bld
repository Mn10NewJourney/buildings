		AddShaftDoorComponent %StartNo% + 1, 1, Right, (%DoorTexture%)2, (%DoorTexture%)2, 0.2, left, 0.6, -2.8, -2, -2.8, 0, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo% + 1, 1, %AutoFiller%, false

	#Door tracks
	AddDoorTrack(1, 1, 4.67 - 0.33 - 0.2, 4.67 - 0.33, 5 - 4, 5 + 4, ElevDoorTrackX)
	AddDoorTrack(2, 1, 4.67 - 0.33 - 0.2, 4.67 - 0.33, -5 - 4, -5 + 4, ElevDoorTrackX)
	AddDoorTrack(3, 2, -4.67 + 0.33, -4.67 + 0.33 + 0.2, 5 - 4, 5 + 4, ElevDoorTrackX)
	AddDoorTrack(4, 2, -4.67 + 0.33, -4.67 + 0.33 + 0.2, -5 - 4, -5 + 4, ElevDoorTrackX)

	#****************************************#

	#Door trims/fillers
	#WallTex() #Set filler textures to match with shaft walls

		#Elevators 1-2
		AddDeco4(%DoorTrimTexture%, %DoorTrimTexture_Top%, right, true, false, floor, 0.05, 0.2, -5, 10, 4, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66)
		AddDeco4(%DoorTrimTexture%, %DoorTrimTexture_Top%, right, true, false, floor, 0.05, 0.2, -5, 0, 4, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66)

