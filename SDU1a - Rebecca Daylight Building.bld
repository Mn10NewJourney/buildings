#SET HALL STATIONS' TEXTURES
<Function ElevHallTex>
	#Shaft doors
		#Passenger elevator
		Set DoorTexture = LobbyElevDoors
		if[%floor% = 1] Set DoorTexture = HangoutElevDoors
		if[%floor% > 1] Set DoorTexture = UpperElevDoors
	
	#Use automatic filler walls or not
	Set AutoFiller = false
	
	#Door trims
		#Sides
		Set DoorTrimTexture = %DoorTexture%_Trim

		#Top
		Set DoorTrimTexture_Top = %DoorTrimTexture%
		if[%floor% = 1] Set DoorTrimTexture_Top = %DoorTexture%_TrimTop

	#Deco dimensions
		#Set SideDecoH = 7.2
		#Set TopDecoH = 0.2
		Set SideDecoH = %height%
		Set TopDecoH = %height% - 7

	#Call button panels
	#Set BPanelTexture = ElevExtPanels
	Set BPanelTexture = ElevExtPanels-Alt
	Set ButtonPrefix = VRButton #Elevators 1-4

	#Lantern panels
	#Set LPanelTexture = %BPanelTexture%
	Set LPanelTexture = ElevExtPanels

		#Lateral offset for lantern panels
		#Set ExtraLateralOffset = 0
		Set ExtraLateralOffset = 0.05
<EndFunction>

#Create hall stations
<Floors 0 to 7>
