<Function ElevHallTex>
	TexPrefix() #Set prefixes for shaft doors
	if[%floor% = 1] Set 5 = Mezzanine #Override for the mezzanine

	#Shaft doors
	Set DoorTexture = %5%ShaftDoors

	#Use automatic filler walls or not
	Set AutoFiller = false
	
	#Deco types 
		#True: Custom door decos
		#False: Filler walls matching with the shaft walls
		
		Set DecoType = true
		if[%floor% = -1 | %floor% > 6] Set DecoType = false

	#Door trims
		#Sides
		Set DoorTrimTexture = %DoorTexture%_Trim

		#Top
		Set DoorTrimTexture_Top = %DoorTrimTexture%
		#if[%floor% = 2 | %floor% = 3] Set DoorTrimTexture_Top = %DoorTexture%_TrimTop

	#Deco dimensions
		Set SideDecoH = %height%
		Set TopDecoH = %height% - 7
		if[%floor% < 0 | %floor% > 3] Set SideDecoH = 7.2
		if[%floor% < 0 | %floor% > 3] Set TopDecoH = 0.2

		#if[%floor% = 2 | %floor% = 3] Set SideDecoH = 8
		#if[%floor% = 2 | %floor% = 3] Set TopDecoH = 1

	#Call button panels
	Set BPanelTexture = ElevExtPanels
	Set ButtonPrefix = T2Button

	#Lantern panels
	Set LPanelTexture = %BPanelTexture%
	Set LanternPrefix = T2Lantern

		#Lateral offset for lantern panels
		Set ExtraLateralOffset = 0
		if[%floor% > -1 & %floor% < 4] Set ExtraLateralOffset = 0.05
<EndFunction>

