<Function AFElev_CeilingLights>
	#CEILING LIGHTS FOR ELEVATORS 1-2 and 5-8
	Set EW = 6 - 0.4 #Width
	Set ED = 9.28 - 0.4 #Depth
	Set ED2 = %ED% / 2


	#1. Ceiling lights
	Set LightOffset = 8 - 0.001
	Set LightOffset2 = 8 - 0.002
	Set GridSuffix = Grid
	if[%elevator% > 4] Set GridSuffix = Grid2

	Set LightWidth = 5.5
	Set LightLength = 5.5
	
	DrawWalls = true, false, false, false, false, false
	AddFloor CeilingLight, %Prefix%ElevCeilingLight, 0, 0 - (%LightWidth% / 2), -(%LightLength% / 2), 0 + (%LightWidth% / 2), (%LightLength% / 2), %LightOffset%, %LightOffset%, 0, 0
	AddFloor CeilingGrid, %Prefix%ElevCeiling%GridSuffix%, 0, 0 - (%LightWidth% / 2), -(%LightLength% / 2), 0 + (%LightWidth% / 2), (%LightLength% / 2), %LightOffset2%, %LightOffset2%, %LightWidth% / 0.5, %LightLength% / 0.5

	#2. Downlights
	DrawWalls = true, false, false, false, false, false
	Set DLOffset = 8 - 0.001 #Offsets for downlights
	AddFloor Downlight1/1, %Prefix%ElevCeilingDL, 0, -2.25 - 0.095, (-%ED2% + 1) - 0.095, -2.25 + 0.095, (-%ED2% + 1) + 0.095, %DLOffset%, %DLOffset%, 0, 0
	AddFloor Downlight1/2, %Prefix%ElevCeilingDL, 0, 0 - 0.095, (-%ED2% + 1) - 0.095, 0 + 0.095, (-%ED2% + 1) + 0.095, %DLOffset%, %DLOffset%, 0, 0
	AddFloor Downlight1/3, %Prefix%ElevCeilingDL, 0, 2.25 - 0.095, (-%ED2% + 1) - 0.095, 2.25 + 0.095, (-%ED2% + 1) + 0.095, %DLOffset%, %DLOffset%, 0, 0

	AddFloor Downlight2/1, %Prefix%ElevCeilingDL, 0, -2.25 - 0.095, (%ED2% - 1) - 0.095, -2.25 + 0.095, (%ED2% - 1) + 0.095, %DLOffset%, %DLOffset%, 0, 0
	AddFloor Downlight2/2, %Prefix%ElevCeilingDL, 0, 0 - 0.095, (%ED2% - 1) - 0.095, 0 + 0.095, (%ED2% - 1) + 0.095, %DLOffset%, %DLOffset%, 0, 0
	AddFloor Downlight2/3, %Prefix%ElevCeilingDL, 0, 2.25 - 0.095, (%ED2% - 1) - 0.095, 2.25 + 0.095, (%ED2% - 1) + 0.095, %DLOffset%, %DLOffset%, 0, 0
