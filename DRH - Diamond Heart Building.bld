#Set textures (Service)
<Function ElevHallTex_Service>
	TexPrefix() #Set prefixes for filler walls

	#Shaft doors
	Set DoorTexture = ServiceAShaftDoors
	if[%floor% = 15] Set DoorTexture = ServiceBShaftDoors
	if[%floor% = -1] Set DoorTexture = ServiceCShaftDoors

	#Use automatic filler walls or not
	Set AutoFiller = false
	
	#Deco types 
		#True: Custom door decos
		#False: Filler walls matching with the shaft walls
		
		Set DecoType = false
		#Set DecoType = true
		#if[%floor% = -1 | %floor% = 15] Set DecoType = false

	#Door trims
		#Sides
		Set DoorTrimTexture = %DoorTexture%_Trim

		#Top
		Set DoorTrimTexture_Top = %DoorTrimTexture%

	#Deco dimensions
		Set SideDecoH = 7.2
		Set TopDecoH = 0.2

	#Call button panels
	Set BPanelTexture = ElevExtPanels
	Set ButtonPrefix = TKVRButton

	#Lantern panels
	Set LPanelTexture = %BPanelTexture%
	Set LanternPrefix = TKVRLantern

		#Lateral offset for lantern panels
		Set ExtraLateralOffset = 0
		#Set ExtraLateralOffset = 0.05
<EndFunction>

