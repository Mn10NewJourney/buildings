#Door track and top filler functions
<Function AddDoorTrack>
	Set ElevNo = %param1% #Elevator Number
	Set ShaftNo = %param2% #Shaft Number
	Set X1 = %param3% #Door Track Position X1
	Set X2 = %param4% #Door Track Position X2
	Set Z1 = %param5% #Door Track Position Z1
	Set Z2 = %param6% #Door Track Position Z2
	Set Texture = %param7% #Door Track Texture

	FloorOrientation = top
	DrawWalls = true, true, true, true, true, true
	TextureOverride ElevExt, %Texture%, ElevExt, ElevExt, ElevExt, ElevExt
	AddShaftFloor %ShaftNo%, Elev(%ElevNo%)_Track, %Texture%, 0.05, %X1%, %Z1%, %X2%, %Z2%, %interfloorheight%, %interfloorheight%, 0, 0
<EndFunction>

<Function AddTopFiller>
	Set ElevNo = %param1% #Elevator Number
	Set ShaftNo = %param2% #Shaft Number
	Set X1 = %param3% #Door Track Position X1
	Set X2 = %param4% #Door Track Position X2
	Set Z1 = %param5% #Door Track Position Z1
	Set Z2 = %param6% #Door Track Position Z2
	Set DoorHeight = %param7% #Door Height
	Set Texture = %param8% #Top Filler Texture

	FloorOrientation = bottom
	DrawWalls = true, true, true, true, true, true
	TextureOverride %Texture%, ElevExt, ElevExt, ElevExt, ElevExt, ElevExt
	AddShaftFloor %ShaftNo%, Elev(%ElevNo%)_Filler, %Texture%, 0.05, %X1%, %Z1%, %X2%, %Z2%, %interfloorheight% + %DoorHeight%, %interfloorheight% + %DoorHeight%, 0, 0
<EndFunction>

###########################################################

#Exterior and interior
#Set wall textures
<Function WallTex>
	if[%floor% = -1] Set 6 = BasementWall #Wall Texture
	if[%floor% > -1 & %floor% < 7] Set 6 = LobbyWall #Wall Texture
	#if[%floor% > 6 & %floor% < 10] Set 6 = OfficeWall #Wall Texture
	if[%floor% > 6 & %floor% < 9] Set 6 = OfficeWall #Wall Texture
	if[%floor% = 9] Set 6 = OfficeWall2 #Wall Texture
	if[%floor% > 9 & %floor% < 27] Set 6 = WardWall #Wall Texture
	if[%floor% = 27] Set 6 = ServiceWall #Wall Texture
	if[%floor% > 27] Set 6 = ServiceWall2 #Wall Texture

	#Override wall texture for support columns
	Set 6A = %6%
	if[%floor% = -1] Set 6A = ColumnTex
	if[%floor% > 26] Set 6A = ColumnTex2
	
		#Escalator railings
		Set 6E = %6%
		if[%floor% = 1 | %floor% = 2] Set 6E = LobbyEscRail
	
		#Set textures for office walls
		Set 6Int = %6%
		if[%floor% > 6 & %floor% < 10] Set 6Int = OfficeIntWall
		if[%floor% > 9 & %floor% < 27] Set 6Int = WardIntWall

		#Set textures for support columns inside offices/patient rooms
		Set 6B = %6A%
		if[%floor% > 6 & %floor% < 10] Set 6B = OfficeIntWall
		if[%floor% > 9 & %floor% < 27] Set 6B = WardIntWall
		
		#Set textures for support columns that are INSIDE offices but OUTSIDE of patient rooms
		Set 6C = %6A%
		if[%floor% > 6 & %floor% < 10] Set 6C = OfficeIntWall
<EndFunction>

