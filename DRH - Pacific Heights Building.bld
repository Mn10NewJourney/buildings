<EndFloors>

#########################################################################

#Windows on above ground levels
<Floors 0 to 6>
	TexPrefix() #Set texture prefix/suffix

	#Windows creation
	Set MyIFloorType = 2
	if[%floor% = 0] Set MyIFloorType = 3

	Set WinPrefix = Main #Windows' prefixes
	if[%floor% = 0] Set WinPrefix = Lobby
	Set WinPrefix2 = Main

	Set WinHeight = %height%
	if[%floor% = 6] Set WinHeight = %height% + Floor(7).FullHeight

	if[%floor% = 0] Set LobbyWH = %height% + Floor(0).FullHeight
	if[%floor% ! 0] Set LobbyWH = %WinHeight%

	SetAutoSize = true, false
	
		#Front windows
		if[%floor% ! 1] AddCustomWindows(FrontWin1, %WinPrefix2%Windows, %WinPrefix2%WindowsInt, -%1%, -%2%, -30 - 40, -%2%, %LobbyWH%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)
		if[%floor% ! 1] AddCustomWindows(FrontWin2, %WinPrefix%Windows, %WinPrefix%WindowsInt, -30, -%2% - %ExtraSouth%, 30, -%2% - %ExtraSouth%, %LobbyWH%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)
		if[%floor% ! 1] AddCustomWindows(FrontWin3, %WinPrefix2%Windows, %WinPrefix2%WindowsInt, 30, -%2%, %1%, -%2%, %LobbyWH%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)

		#Left windows
		if[%floor% ! 1] AddCustomWindows(LeftWin1, %WinPrefix2%Windows, %WinPrefix2%WindowsInt, -%1%, -%2%, -%1%, -5, %LobbyWH%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)
		AddCustomWindows(LeftWin2, %WinPrefix2%Windows, %WinPrefix2%WindowsInt, -%1%, -5 + 30, -%1%, %2%, %WinHeight%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)

		#Right windows
		if[%floor% ! 1] AddCustomWindows(RightWin_Main1, %WinPrefix2%WindowsInt, %WinPrefix2%Windows, %1%, -%2%, %1%, -5, %LobbyWH%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)
		AddCustomWindows(RightWin_Main2, %WinPrefix2%WindowsInt, %WinPrefix2%Windows, %1%, -5 + 30, %1%, %2%, %WinHeight%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)
		if[%floor% ! 1] AddCustomWindows(RightWin_Extra, %WinPrefix%WindowsInt, %WinPrefix%Windows, 30, -%2% - %ExtraSouth%, 30, -%2%, %LobbyWH%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)

		#Back windows
		AddCustomWindows(BackWin1, %WinPrefix2%WindowsInt, %WinPrefix2%Windows, -%1%, %2%, -30, %2%, %WinHeight%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)
		AddCustomWindows(BackWin2, %WinPrefix2%WindowsInt, %WinPrefix2%Windows, -5, %2%, 5, %2%, %WinHeight%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)
		AddCustomWindows(BackWin3, %WinPrefix2%WindowsInt, %WinPrefix2%Windows, 30, %2%, %1%, %2%, %WinHeight%, 0, 0, %MyIFloorType%, ExtFacade, %5%Wall)

	SetAutoSize = true, true
<EndFloors>

#Exterior perimeter walls
<Floors 0 to 6>
