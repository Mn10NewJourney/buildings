		Set ExtraSouth = 30
		
		#Wall suffixes for lobby areas (center hallway)
		Set WallSuffix = Wall2
		if[%floor% = 0 | %floor% = 1] Set WallSuffix = Wall

		#Wall suffixes for elevator lobbies (Levels B3-B1 and 2-5 only)
		Set WallSuffix2 = %WallSuffix%
		if[%floor% > 1 & %floor% < 6] Set WallSuffix2 = Wall3
		if[%floor% < 0] Set WallSuffix2 = Wall4

		#Suffixes for support columns on B3-B1
		Set SupportSuffix = Wall
		if[%floor% < 0] Set SupportSuffix = Wall3

	#Shaft doors for service elevators (if added later)
	#Set 5S = ServiceA
	#Set 5S = ServiceB
<EndFunction>


