<Function BtnPanelE1-3>
	Set PanelNumber = %param1%
	Set PanelNumber2 = %PanelNumber% + 1
	Set ButtonPrefix = T4VRButton

	AddButton %PanelNumber%, , %ButtonPrefix%6, %ButtonPrefix%Lit6, 1, 2, 6, 1, 1
	AddButton %PanelNumber%, , %ButtonPrefix%5, %ButtonPrefix%Lit5, 1, 1, 5, 1, 1
	AddButton %PanelNumber%, , %ButtonPrefix%4, %ButtonPrefix%Lit4, 2, 2, 4, 1, 1
	AddButton %PanelNumber%, , %ButtonPrefix%3, %ButtonPrefix%Lit3, 2, 1, 3, 1, 1
	AddButton %PanelNumber%, , %ButtonPrefix%2, %ButtonPrefix%Lit2, 3, 2, 2, 1, 1
	AddButton %PanelNumber%, , %ButtonPrefix%1, %ButtonPrefix%Lit1, 3, 1, 0, 1, 1
	AddButton %PanelNumber%, , MainFloorStar, MainFloorStar, 3, 1, off, 0.4, 0.8, -0.75, 0

	AddButton %PanelNumber%, , %ButtonPrefix%B1, %ButtonPrefix%LitB1, 4, 2, -1, 1, 1
	AddButton %PanelNumber%, , %ButtonPrefix%B2, %ButtonPrefix%LitB2, 4, 1, -2, 1, 1
	AddButton %PanelNumber2%, , %ButtonPrefix%B3, %ButtonPrefix%LitB3, 5, 1, -3, 1, 1

	AddButton %PanelNumber%, , %ButtonPrefix%Open, %ButtonPrefix%Open, 7, 1, open, 1, 1
	AddButton %PanelNumber%, , %ButtonPrefix%Close, %ButtonPrefix%Close, 7, 2, close, 1, 1
	if[%PanelNumber% = 1] AddButton %PanelNumber%, , %ButtonPrefix%Alarm, %ButtonPrefix%Alarm, 8, 2, alarm, 1, 1
<EndFunction>

<Elevators 1 to 3>
	Set Prefix = South

	#Set textures and dimensions
	Set P1Width = 0.25
	Set P2Width = 1.25

	M10_ElevCabTex(ElevExt, %Prefix%ElevFloor, %Prefix%ElevCeiling, %Prefix%ElevWall, %Prefix%ElevWall)
	M10_ElevDimensions(6, 9.28, 8, 0.2, 0.2, true, true, true, false)
	M10_ElevPanels(%Prefix%ElevPanelMain, %Prefix%ElevPanelTop, %P1Width%, %P2Width%, 1, 0.2, false, false, false, true)

	#Finish elevator assembly
	M10_ElevInterior()
	#--------------------
	#MORE DETAILED INTERIORS

	#1. Panels on walls
	Set WPTex = %Prefix%ElevWallPanel
	Set WPanelHeight = 8 - 0.5

		#Front panels
		CreateWallBox2 elevator, Panel_F1, %WPTex%, 0 - (5.6 * 1/4), -4.44 + 0.01, (5.6 * 1/2) - 0.3, 0.02, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_F2, %WPTex%, 0 + (5.6 * 1/4), -4.44 + 0.01, (5.6 * 1/2) - 0.3, 0.02, %WPanelHeight%, 0.25, 0, 0, false, true, true, true

		#Left panels
		CreateWallBox2 elevator, Panel_L1, %WPTex%, -2.8 + 0.01, 0 - (8.88 * 1/3), 0.02, (8.88 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_L2, %WPTex%, -2.8 + 0.01, 0, 0.02, (8.88 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_L3, %WPTex%, -2.8 + 0.01, 0 + (8.88 * 1/3), 0.02, (8.88 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true

		#Right panels
		CreateWallBox2 elevator, Panel_R1, %WPTex%, 2.8 - 0.01, 0 - (8.88 * 1/3), 0.02, (8.88 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_R2, %WPTex%, 2.8 - 0.01, 0, 0.02, (8.88 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_R3, %WPTex%, 2.8 - 0.01, 0 + (8.88 * 1/3), 0.02, (8.88 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true

	#2. Ceiling lights
	Set LightWidth = 2.375
	Set LightLength = 9.28 - 0.4
	Set TrimWidth = 0.2

	CreateWallBox2 elevator, CL_Main, %Prefix%ElevCeilingLight, 0, 0, %LightWidth%, %LightLength%, 0.035, 8 - 0.035, 0, 0, false, true, true, true

	#2a. Trims around lights
	CreateWallBox2 elevator, CL_Trim_Left, %Prefix%ElevCeilingTrim, 0 - (%LightWidth% / 2) - (%TrimWidth% / 2), 0, %TrimWidth%, %LightLength%, 0.035, 8 - 0.035, 0, 0, false, true, true, true
	CreateWallBox2 elevator, CL_Trim_Right, %Prefix%ElevCeilingTrim, 0 + (%LightWidth% / 2) + (%TrimWidth% / 2), 0, %TrimWidth%, %LightLength%, 0.035, 8 - 0.035, 0, 0, false, true, true, true

	#3. Downlights
	DrawWalls = true, false, false, false, false, false
	Set DLOffset = 8 - 0.001 #Offsets for downlights
	AddFloor Downlight1/1, %Prefix%ElevCeilingDL, 0, -2.8 + 0.75 - 0.11, (-3 - 0.2) - 0.11, -2.8 + 0.75 + 0.11, (-3 - 0.2) + 0.11, %DLOffset%, %DLOffset%, 0, 0
	AddFloor Downlight1/2, %Prefix%ElevCeilingDL, 0, 2.8 - 0.75 - 0.11, (-3 - 0.2) - 0.11, 2.8 - 0.75 + 0.11, (-3 - 0.2) + 0.11, %DLOffset%, %DLOffset%, 0, 0
	AddFloor Downlight2/1, %Prefix%ElevCeilingDL, 0, -2.8 + 0.75 - 0.11, 0 - 0.11, -2.8 + 0.75 + 0.11, 0 + 0.11, %DLOffset%, %DLOffset%, 0, 0
	AddFloor Downlight2/2, %Prefix%ElevCeilingDL, 0, 2.8 - 0.75 - 0.11, 0 - 0.11, 2.8 - 0.75 + 0.11, 0 + 0.11, %DLOffset%, %DLOffset%, 0, 0
	AddFloor Downlight3/2, %Prefix%ElevCeilingDL, 0, -2.8 + 0.75 - 0.11, (3 + 0.2) - 0.11, -2.8 + 0.75 + 0.11, (3 + 0.2) + 0.11, %DLOffset%, %DLOffset%, 0, 0
	AddFloor Downlight3/2, %Prefix%ElevCeilingDL, 0, 2.8 - 0.75 - 0.11, (3 + 0.2) - 0.11, 2.8 - 0.75 + 0.11, (3 + 0.2) + 0.11, %DLOffset%, %DLOffset%, 0, 0

	#--------------------
	#Car Doors
	Set ElevDoorTex = %Prefix%ElevDoors
	Set IDP = 4.64

	WallOrientation = left
	AddDoorComponent 1, 2Speed_Fast, %ElevDoorTex%2, %ElevDoorTex%2, 0.15, right, 0.8, 0.6, -2.75, %IDP% + (0.15 * 1), -2.75 + (2.25 * 1), %IDP% + (0.15 * 1), 7, 0, 0, 0, 0, 0
	AddDoorComponent 1, 2Speed_Slow, %ElevDoorTex%2, %ElevDoorTex%2, 0.15, right, 0.4, 0.3, -2.75 + (2.25 * 1), %IDP%, -2.75 + (2.25 * 2), %IDP%, 7, 0, 0, 0, 0, 0
	FinishDoors 1, false, false

		#Door track
		FloorOrientation = top
		DrawWalls = true, true, true, true, true, true
		TextureOverride ElevExt, ElevDoorTrackZ, ElevExt, ElevExt, ElevExt, ElevExt
		AddFloor DoorTrack, ElevDoorTrackZ, 0.05, -2.75, %IDP%, -2.75 + (2.25 * 3), %IDP% + (0.15 * 2), 0, 0, 0, 0

		#Top filler
		FloorOrientation = bottom
		DrawWalls = true, true, true, true, true, true
		TextureOverride %Prefix%ElevPanelTop, ElevExt, ElevExt, ElevExt, ElevExt, ElevExt
		AddFloor TopFiller, %Prefix%ElevPanelTop, 0.05, -2.75, %IDP%, -2.75 + (2.25 * 1), %IDP% + (0.15 * 1), 7, 7, 0, 0

	#Gap filler
	WallOrientation = left
	DrawWalls = true, true, true, true, true, true
	TextureOverride ElevExt, ElevExt, ElevExt, %Prefix%ElevPanelMain, ElevExt, ElevExt
	AddWall GapFiller, %Prefix%ElevPanelMain, 0.15 * 2, -3, %IDP%, -2.75, %IDP%, 7, 7, 0, 0, 0, 0
	#--------------------
	#Button Panels
	Set P1_PositionX = 2.275
	Set P1_PositionZ = 4.44 - 0.051

		#Panel back
		CreateWallBox2 elevator, MainPB, ElevPanel, %P1_PositionX%, %P1_PositionZ% + 0.051 - 0.025, 0.8, 0.05, 4.875, 2, 0, 0, false, true, true, true

		#Panel
		CreatePanel , 8, 2, front, %P1_PositionX%, %P1_PositionZ%, 0.24, 0.12, 0.3, 0.3, 3.5, 0, 0
		CreatePanel , 8, 1, front, %P1_PositionX%, %P1_PositionZ%, 0.24, 0.12, 0.3, 0.3, 3.5, 0, 0 #Level B3 button only

			#Buttons
			BtnPanelE1-3(1)

	#Elevator number sign
	DrawWalls = true, false, false, false, false, false
	AddWall ElevNo%elevator%_Inside, IN_ElevNo%elevator%, 0, -0.5 - 0.25, 4.44 - 0.001, -0.5 + 0.25, 4.44 - 0.001, 0.5, 0.5, 7.05, 7.05, 0, 0

	#Indicator sets
	AddFloorIndicator T4VRInd_Button, front, %P1_PositionX%, %P1_PositionZ% - 0.001, 0.6, 0.3, 5.75 + 0.25 - (0.3 / 2)
	AddDirectionalIndicator true, true, true, Blank, T4VRArrowUp, T4VRArrowUpLit, T4VRArrowDown, T4VRArrowDownLit, %P1_PositionX% - (0.3 / 2), %P1_PositionZ% - 0.001, 5.75 + 0.25 - (0.3 / 2), front, 0.3, 0.3, true, 0, 0
	#--------------------
	#Shaft components
	DrawWalls = true, true, true, true, true, true
		#Ropes
		RopePosition = 0, 8 + 0.2, 0
		RopeTexture = Ropes
	
		#Counterweight
		Set Position = 3 + 0.8
		CreateCounterweight CWeight_Frame, CWeight, %Position%, 0, 0.5, 6, 2, 0.5
	
		#Rails
		AddRails ElevRail, ElevRailSide, 0, 0, false, 3 + 0.06, 0.1
		AddRails ElevRail, ElevRailSide, %Position%, 0, true, 1 + 0.06, 0.1
<EndElevators>

