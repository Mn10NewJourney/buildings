#Sets for all walls
<Function ExtSHSets> #Levels B3 to 6
	#Set textures
	TexPrefix()

	#Set heights
	Set WH = %height%
	Set WO = 0
	
		if[%floor% = 1 | %floor% = 7] Set WH = %fullheight%
		if[%floor% = 1 | %floor% = 7] Set WO = -%interfloorheight%
<EndFunction>

<Function ExtSHSets_RF> #Roof only
	#Set heights
	Set WH = %fullheight%
	Set WO = 0
<EndFunction>

### Shaft and stairwell walls
<Floors -3 to 7>
	ExtSHSets() #Set parameters

	#North Elevators
	CreateShaftWall(%5%%WallSuffix%, %5%%WallSuffix%, %5%%WallSuffix%, %5%Wall, %5%Wall, %5%Wall, 0.33, 11, 25, 6, 15, N-Elev-SH, %WH%, %WO%, true, true, true, false, false, false, false)
	
	#South Elevators
	CreateShaftWall(%5%Wall, %5%Wall, %5%%WallSuffix%, %5%%WallSuffix%, %5%Wall, %5%Wall, 0.33, -45, -64, 15, 6, S-Elev-SH, %WH%, %WO%, false, false, true, true, false, false, false)

	####################

	#North Stairwell
	CreateShaftWall(%5%%WallSuffix%, %5%%WallSuffix%, %5%%WallSuffix%, %5%Wall, %5%Wall, %5%Wall, 0.5, -11, 25, 6, 15, NorthST, %WH%, %WO%, true, true, true, false, false, false, false)

	#South Stairwell
	CreateShaftWall(%5%Wall, %5%Wall, %5%%WallSuffix%, %5%Wall, %5%Wall, %5%Wall, 0.5, -65, -55, 5, 15, SouthST, %WH%, %WO%, false, false, true, false, false, false, false)
	
		WallOrientation = left
		DrawWalls = false, true, true, true, false, false
		AddWall SouthSTBack_Extra, %5%Wall, 0.5, -65 - 5, -40, -65 + 5, -40, %WH%, %WH%, %WO%, %WO%, 0, 0, false

	#West Stairwell
	CreateShaftWall(%5%Wall, %5%Wall, %5%Wall, %5%Wall, %5%Wall, %5%Wall, 0.5, -100, -5 + 15, 5, 15, WestST, %WH%, %WO%, true, false, true, true, false, false, false)

	#East Stairwell
	CreateShaftWall(%5%Wall, %5%Wall, %5%Wall, %5%Wall, %5%Wall, %5%Wall, 0.5, 100, -5 + 15, 5, 15, WestST, %WH%, %WO%, true, true, false, true, false, false, false)
<EndFloors>

<Floor 8>
	ExtSHSets_RF() #Set parameters

	#North Elevators
	CreateShaftWall(ExtFacade, ExtFacade, ExtFacade, ExtFacade, ExtFacade, %5%Wall, 0.33, 11, 25, 6, 15, N-Elev-SH_RF, %WH%, %WO%, true, true, true, true, true, false, true)
	
	#South Elevators
	CreateShaftWall(ExtFacade, ExtFacade, ExtFacade, ExtFacade, ExtFacade, %5%Wall, 0.33, -45, -64, 15, 6, S-Elev-SH_RF, %WH%, %WO%, true, true, true, true, true, false, true)

	####################

	#North Stairwell
	CreateShaftWall(ExtFacade, ExtFacade, ExtFacade, ExtFacade, ExtFacade, %5%Wall, 0.5, -11, 25, 6, 15, NorthST_RF, %WH%, %WO%, true, true, true, true, true, false, true)

	#South Stairwell
	CreateShaftWall(ExtFacade, ExtFacade, ExtFacade, ExtFacade, ExtFacade, %5%Wall, 0.5, -65, -55, 5, 15, SouthST_RF, %WH%, %WO%, true, true, true, true, true, false, true)
	
		WallOrientation = left
		DrawWalls = false, true, true, true, true, false
		AddWall SouthSTBack_Extra_RF, ExtFacade, 0.5, -65 - 5, -40, -65 + 5, -40, %WH%, %WH%, %WO%, %WO%, 0, 0, true
<EndFloor>

