<Function TexPrefix>
	#Main prefixes
	Set 5 = Hospital
	if[%floor% < 0] Set 5 = Basement
	if[%floor% = 0 | %floor% = 1] Set 5 = Lobby
	if[%floor% = 6 | %floor% = 7] Set 5 = 6F

