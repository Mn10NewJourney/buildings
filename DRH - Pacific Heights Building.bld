	AddShaftDoorComponent %StartNo% + 1, 1, 2Speed_Slow, %DoorTexture%2, %DoorTexture%2, 0.15, right, 0.4, 0.3, -2.75 + (2.25 * 1), %IDP% + (0.15 * 1), -2.75 + (2.25 * 2), %IDP% + (0.15 * 1), 7, 0, -1, 0, -1, 0
	FinishShaftDoor %StartNo% + 1, 1, %AutoFiller%, false

	WallOrientation = left
	AddShaftDoorComponent %StartNo% + 2, 1, 2Speed_Fast, %DoorTexture%, %DoorTexture%, 0.15, right, 0.8, 0.6, -2.75, %IDP%, -2.75 + (2.25 * 1), %IDP%, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo% + 2, 1, 2Speed_Slow, %DoorTexture%2, %DoorTexture%2, 0.15, right, 0.4, 0.3, -2.75 + (2.25 * 1), %IDP% + (0.15 * 1), -2.75 + (2.25 * 2), %IDP% + (0.15 * 1), 7, 0, -1, 0, -1, 0
	FinishShaftDoor %StartNo% + 2, 1, %AutoFiller%, false

	#Door tracks
	AddShaftTrack(1, 1, 9 - 2.25, 9 + 4.5, 5.34 - 0.3, 5.34, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)
	AddShaftTrack(2, 1, -1 - 2.25, -1 + 4.5, 5.34 - 0.3, 5.34, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)
	AddShaftTrack(3, 1, -11 - 2.25, -11 + 4.5, 5.34 - 0.3, 5.34, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)

	#Top filler
	AddShaftTrack(1, 1, 9 - 2.25, 9, 5.34 - 0.15, 5.34, %DoorTrimTexture%, ElevExt, 0.05, 2, 7)
	AddShaftTrack(2, 1, -1 - 2.25, -1, 5.34 - 0.15, 5.34, %DoorTrimTexture%, ElevExt, 0.05, 2, 7)
	AddShaftTrack(3, 1, -11 - 2.25, -11, 5.34 - 0.15, 5.34, %DoorTrimTexture%, ElevExt, 0.05, 2, 7)

	#Gap filler
		WallOrientation = right
		DrawWalls = true, true, true, true, true, true
		TextureOverride ElevExt, ElevExt, %DoorTrimTexture%, %DoorTrimTexture%, ElevExt, ElevExt
		AddShaftWall 1, GapFiller_Elev1, ElevExt, 0.15 * 2, 9 - 2.25 - 0.25, 5.34, 9 - 2.25, 5.34, 7, 7, %interfloorheight%, %interfloorheight%, 0, 0	
		TextureOverride ElevExt, ElevExt, %DoorTrimTexture%, %DoorTrimTexture%, ElevExt, ElevExt
		AddShaftWall 1, GapFiller_Elev2, ElevExt, 0.15 * 2, -1 - 2.25 - 0.25, 5.34, -1 - 2.25, 5.34, 7, 7, %interfloorheight%, %interfloorheight%, 0, 0	
		TextureOverride ElevExt, ElevExt, %DoorTrimTexture%, %DoorTrimTexture%, ElevExt, ElevExt
		AddShaftWall 1, GapFiller_Elev3, ElevExt, 0.15 * 2, -11 - 2.25 - 0.25, 5.34, -11 - 2.25, 5.34, 7, 7, %interfloorheight%, %interfloorheight%, 0, 0	

	#****************************************#

	#Door trims/fillers
	TexPrefix() #Reset texture prefixes
	Set Shaft1CX = -45 #1st shaft for elevator bank
	Set Shaft1CZ = -64

		#1st-6th floors
		if[%floor% > -1] AddDecoObject(Elev1_Deco, %DoorTrimTexture%, %DoorTrimTexture_Top%, back, true, false, 0.05, 0.2, %Shaft1CX% + 9, %Shaft1CZ% + 6, 4.5, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66, floor)
		if[%floor% > -1] AddDecoObject(Elev2_Deco, %DoorTrimTexture%, %DoorTrimTexture_Top%, back, true, false, 0.05, 0.2, %Shaft1CX% - 1, %Shaft1CZ% + 6, 4.5, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66, floor)
		if[%floor% > -1] AddDecoObject(Elev3_Deco, %DoorTrimTexture%, %DoorTrimTexture_Top%, back, true, false, 0.05, 0.2, %Shaft1CX% - 11, %Shaft1CZ% + 6, 4.5, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66, floor)

		#Basements
		WallOrientation = right
		if[%floor% < 0] AddFillerWalls %5%%WallSuffix2%, 0.66, %Shaft1CX% + 9, %Shaft1CZ% + 6, 4.5, 7, 0, true, 0, 0, false #Elevator 1
		if[%floor% < 0] AddFillerWalls %5%%WallSuffix2%, 0.66, %Shaft1CX% - 1, %Shaft1CZ% + 6, 4.5, 7, 0, true, 0, 0, false #Elevator 2
		if[%floor% < 0] AddFillerWalls %5%%WallSuffix2%, 0.66, %Shaft1CX% - 11, %Shaft1CZ% + 6, 4.5, 7, 0, true, 0, 0, false #Elevator 3

		#Extra decos on the 2nd-5th floors
		if[%floor% > 1 & %floor% < 6] AddDecoObject(E1_ExtraDecos, %DoorTexture%_Deco, %DoorTexture%_Deco, back, true, false, 0.01, 1 - 0.2, %Shaft1CX% + 9, %Shaft1CZ% + 6, 4.5 + 0.4, 7 + 1, %height%, %height% - 8, 0, false, %DoorTexture%_Deco, 0.66, floor)
		if[%floor% > 1 & %floor% < 6] AddDecoObject(E2_ExtraDecos, %DoorTexture%_Deco, %DoorTexture%_Deco, back, true, false, 0.01, 1 - 0.2, %Shaft1CX% - 1, %Shaft1CZ% + 6, 4.5 + 0.4, 7 + 1, %height%, %height% - 8, 0, false, %DoorTexture%_Deco, 0.66, floor)
		if[%floor% > 1 & %floor% < 6] AddDecoObject(E3_ExtraDecos, %DoorTexture%_Deco, %DoorTexture%_Deco, back, true, false, 0.01, 1 - 0.2, %Shaft1CX% - 11, %Shaft1CZ% + 6, 4.5 + 0.4, 7 + 1, %height%, %height% - 8, 0, false, %DoorTexture%_Deco, 0.66, floor)

	#****************************************#

	#Call buttons
	CallButtonElevators = 1, 2, 3
	CreateCallButtons %BPanelTexture%, %ButtonPrefix%Up, %ButtonPrefix%UpLit, %ButtonPrefix%Down, %ButtonPrefix%DownLit, %Shaft1CX% + 5 - 1, %Shaft1CZ% + 6.001, 3.5, back, 0.5, 1, true, 0, 0
	CreateCallButtons %BPanelTexture%, %ButtonPrefix%Up, %ButtonPrefix%UpLit, %ButtonPrefix%Down, %ButtonPrefix%DownLit, %Shaft1CX% - 5 - 1, %Shaft1CZ% + 6.001, 3.5, back, 0.5, 1, true, 0, 0

		#Signs
		DrawWalls = false, true, false, false, false, false
		AddWall E1-3HallSign, FireSign, 0, %Shaft1CX% + 4 - 0.25, %Shaft1CZ% + 6.001, %Shaft1CX% + 4 + 0.25, %Shaft1CZ% + 6.001, 0.75, 0.75, 2.6, 2.6, 0, 0, false
		AddWall E1-3FloorSign1, ELFlrSign%floorid%, 0, %Shaft1CX% + 4 - 0.5, %Shaft1CZ% + 6.001, %Shaft1CX% + 4 + 0.5, %Shaft1CZ% + 6.001, 1, 1, 6 - 0.5, 6 - 0.5, 0, 0, false
		AddWall E1-3FloorSign2, ELFlrSign%floorid%, 0, %Shaft1CX% - 6 - 0.5, %Shaft1CZ% + 6.001, %Shaft1CX% - 6 + 0.5, %Shaft1CZ% + 6.001, 1, 1, 6 - 0.5, 6 - 0.5, 0, 0, false

	#****************************************#

	#Indicator sets
	Set Offset = 7.5
	if[%floor% > 1] Set Offset = 7.25

		#Set textures
		ACMEInd_LanternTex(%LanternPrefix%Up, %LanternPrefix%UpLit, %LanternPrefix%Down, %LanternPrefix%DownLit, %LPanelTexture%, T4VR_OInd_Button)

		#Indicator creation
		if[%floor% = 0] ACMELobbyInds(back, 1, %Shaft1CX% + 9, %Shaft1CZ% + 6 + %ExtraLateralOffset%, %Offset%, -3, 6)
		if[%floor% = 0] ACMELobbyInds(back, 2, %Shaft1CX% - 1, %Shaft1CZ% + 6 + %ExtraLateralOffset%, %Offset%, -3, 6)
		if[%floor% = 0] ACMELobbyInds(back, 3, %Shaft1CX% - 11, %Shaft1CZ% + 6 + %ExtraLateralOffset%, %Offset%, -3, 6)

		if[%floor% ! 0] ACMELobbyLanterns(back, 1, %Shaft1CX% + 9, %Shaft1CZ% + 6 + %ExtraLateralOffset%, %Offset%, -3, 6)
		if[%floor% ! 0] ACMELobbyLanterns(back, 2, %Shaft1CX% - 1, %Shaft1CZ% + 6 + %ExtraLateralOffset%, %Offset%, -3, 6)
		if[%floor% ! 0] ACMELobbyLanterns(back, 3, %Shaft1CX% - 11, %Shaft1CZ% + 6 + %ExtraLateralOffset%, %Offset%, -3, 6)

	#****************************************#

	#Elevator number signs
	DrawWalls = false, true, false, false, false, false
	if[%floor% = 0] AddWall E1Sign, ElevNo1, 0, (%Shaft1CX% + 9 + 1.25) - 0.25, %Shaft1CZ% + 6 + %ExtraLateralOffset% + 0.001, (%Shaft1CX% + 9 + 1.25) + 0.25, %Shaft1CZ% + 6 + %ExtraLateralOffset% + 0.001, 0.5, 0.5, %Offset%, %Offset%, 0, 0, false
	if[%floor% = 0] AddWall E2Sign, ElevNo2, 0, (%Shaft1CX% - 1 + 1.25) - 0.25, %Shaft1CZ% + 6 + %ExtraLateralOffset% + 0.001, (%Shaft1CX% - 1 + 1.25) + 0.25, %Shaft1CZ% + 6 + %ExtraLateralOffset% + 0.001, 0.5, 0.5, %Offset%, %Offset%, 0, 0, false
	if[%floor% = 0] AddWall E3Sign, ElevNo3, 0, (%Shaft1CX% - 11 + 1.25) - 0.25, %Shaft1CZ% + 6 + %ExtraLateralOffset% + 0.001, (%Shaft1CX% - 11 + 1.25) + 0.25, %Shaft1CZ% + 6 + %ExtraLateralOffset% + 0.001, 0.5, 0.5, %Offset%, %Offset%, 0, 0, false
