	#****************************************#

	#Door trims/fillers
	TexPrefix() #Reset texture prefixes
	Set Shaft1CX = -45 #1st shaft for elevator bank
	Set Shaft1CZ = -64

		#1st-6th floors
		if[%floor% > -1] AddDecoObject(Elev1_Deco, %DoorTrimTexture%, %DoorTrimTexture_Top%, back, true, false, 0.05, 0.2, %Shaft1CX% + 9, %Shaft1CZ% + 6, 4.5, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66, floor)
		if[%floor% > -1] AddDecoObject(Elev2_Deco, %DoorTrimTexture%, %DoorTrimTexture_Top%, back, true, false, 0.05, 0.2, %Shaft1CX% - 1, %Shaft1CZ% + 6, 4.5, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66, floor)
		if[%floor% > -1] AddDecoObject(Elev3_Deco, %DoorTrimTexture%, %DoorTrimTexture_Top%, back, true, false, 0.05, 0.2, %Shaft1CX% - 11, %Shaft1CZ% + 6, 4.5, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66, floor)

		#Basements
		WallOrientation = right
		if[%floor% < 0] AddFillerWalls %5%%WallSuffix%, 0.66, %Shaft1CX% + 9, %Shaft1CZ% + 6, 4.5, 7, 0, true, 0, 0, false #Elevator 1
		if[%floor% < 0] AddFillerWalls %5%%WallSuffix%, 0.66, %Shaft1CX% - 1, %Shaft1CZ% + 6, 4.5, 7, 0, true, 0, 0, false #Elevator 2
		if[%floor% < 0] AddFillerWalls %5%%WallSuffix%, 0.66, %Shaft1CX% - 11, %Shaft1CZ% + 6, 4.5, 7, 0, true, 0, 0, false #Elevator 3

