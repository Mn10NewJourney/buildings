##--------------------------------------------------------------------------------##

#Set texture prefixes/suffixes
<Function TexPrefix>
	#Main prefixes
	Set 5 = Hospital
	if[%floor% < 0] Set 5 = Basement
	if[%floor% = 6 | %floor% = 7] Set 5 = 6F

		#Add "ExtraSouth" value for floor section.
		Set ExtraSouth = 30
		
	#Shaft doors for service elevators (if added later)
	#Set 5S = ServiceA
	#Set 5S = ServiceB
<EndFunction>


##--------------------------------------------------------------------------------##

#Exterior & interior
#All floors
<Floors -3 to 7>
	TexPrefix() #Set texture prefix/suffix
	Set IsExternal = false

	#Floors
	Set FloorOffset = 0
	#if[%floor% = 1] Set FloorOffset = %interfloorheight%

	FloorOrientation = top
	DrawWalls = false, true, false, false, false, false
	if[%floor% ! 1 & %floor% ! 7] AddFloor %floorid%F_Floor1, %5%Floor, 0.5, -30 - 40, -%2% - %ExtraSouth%, 30, -%2%, %FloorOffset%, %FloorOffset%, 0, 0, %IsExternal%
	if[%floor% ! 1 & %floor% ! 7] AddFloor %floorid%F_Floor2, %5%Floor, 0.5, -%1%, -%2%, %1%, %2%, %FloorOffset%, %FloorOffset%, 0, 0, %IsExternal%

	#Ceilings
	Set CeiOffset = %height%
	#if[%floor% = 1] Set CeiOffset = %fullheight%

	FloorOrientation = bottom
	DrawWalls = true, false, false, false, false, false
	if[%floor% ! 0 & %floor% ! 6] AddFloor %floorid%F_Ceiling1, %5%Ceiling, 0.5, -30 - 40, -%2% - %ExtraSouth%, 30, -%2%, %CeiOffset%, %CeiOffset%, 0, 0, %IsExternal%
	if[%floor% ! 0 & %floor% ! 6] AddFloor %floorid%F_Ceiling2, %5%Ceiling, 0.5, -%1%, -%2%, %1%, %2%, %CeiOffset%, %CeiOffset%, 0, 0, %IsExternal%
<EndFloors>

