	Set BPanelTexture = ElevExtPanels
	if[%floor% > 1 & %floor% < 6] Set BPanelTexture = ElevExtPanels2
	Set ButtonPrefix = T4VRButton

	#Lantern panels
	Set LPanelTexture = %BPanelTexture%
	#if[%floor% > 1 & %floor% < 6] Set LPanelTexture = ElevExtPanels2
	Set LanternPrefix = T4VRLantern

		#Lateral offset for lantern panels
