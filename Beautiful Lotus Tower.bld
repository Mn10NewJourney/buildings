	if[%floor% = 97 | %floor% = 103 | %floor% = 105] Set DoorTx = Door1
<EndFunction>

<Function STARestrooms>
	#STA = Standalone
	#The restrooms' doors are on either west or east of their boundary.
	Set CtrX = %param1%
	Set CtrZ = %param2%
	Set RName = %param3%
	Set DTex = %param4%
	Set DSTex = %param5%
	Set DX = %param6%
	Set DZ = %param7%
	Set Sign1 = %param8% #South doors
	Set Sign2 = %param9% #North doors

	#Internal use only
	Set DimX = %DX% / 2
	Set DimZ = %DZ% / 2
	
	#Restroom walls
	Set RHeight = %height%
	if[%floor% = 1 | %floor% = 49] Set RHeight = %fullheight%
	Set ROffset = 0
	if[%floor% = 1 | %floor% = 49] Set ROffset = -%interfloorheight%

	WallOrientation = center
	DrawWalls = true, true, false, false, false, false
	AddWall %RName%_Mid, %6%, 1, %CtrX% - %DimX%, %CtrZ%, %CtrX% + %DimX%, %CtrZ%, %RHeight%, %RHeight%, %ROffset%, %ROffset%, 0, 0, false

	WallOrientation = left
	AddWall %RName%_Front, %6%, 0.5, %CtrX% - %DimX%, %CtrZ% - %DimZ%, %CtrX% + %DimX%, %CtrZ% - %DimZ%, %RHeight%, %RHeight%, %ROffset%, %ROffset%, 0, 0, false
	AddWall %RName%_Left, %6%, 0.5, %CtrX% - %DimX%, %CtrZ% - %DimZ%, %CtrX% - %DimX%, %CtrZ% + %DimZ%, %RHeight%, %RHeight%, %ROffset%, %ROffset%, 0, 0, false
	WallOrientation = right
	AddWall %RName%_Right, %6%, 0.5, %CtrX% + %DimX%, %CtrZ% - %DimZ%, %CtrX% + %DimX%, %CtrZ% + %DimZ%, %RHeight%, %RHeight%, %ROffset%, %ROffset%, 0, 0, false
	AddWall %RName%_Back, %6%, 0.5, %CtrX% - %DimX%, %CtrZ% + %DimZ%, %CtrX% + %DimX%, %CtrZ% + %DimZ%, %RHeight%, %RHeight%, %ROffset%, %ROffset%, 0, 0, false

		#Restroom ceiling
		Set RestroomHeight = 8.5
		if[%floor% = 1] Set RestroomHeight = 9.5

		FloorOrientation = bottom
		DrawWalls = true, false, false, false, false, false
		AddFloor %RName%_Ceiling, DropCeiling, 0.5, %CtrX% - %DimX%, %CtrZ% - %DimZ%, %CtrX% + %DimX%, %CtrZ% + %DimZ%, %RestroomHeight%, %RestroomHeight%, 0, 0, false

	#Restroom doors
		#West doors
		WallOrientation = right
		TextureOverride %DTex%, %DTex%, %DSTex%, %DSTex%, %DSTex%, %DSTex%
		if[%floor% ! 0 & %floor% ! 1 & %floor% ! 48 & %floor% ! 49 & %floor% < 96] AddDoor , , false, %DTex%, 0.2, 3, 75, %CtrX% - %DimX% + 0.5, %CtrZ% - 2.5, 3, 7, 0, 0, 0
		TextureOverride %DTex%, %DTex%, %DSTex%, %DSTex%, %DSTex%, %DSTex%
		if[%floor% ! 0 & %floor% ! 1 & %floor% ! 48 & %floor% ! 49 & %floor% < 96] AddDoor , , false, %DTex%, 0.2, 2, 75, %CtrX% - %DimX% + 0.5, %CtrZ% + 2.5, 3, 7, 0, 0, 0

		if[%floor% ! 0 & %floor% ! 1 & %floor% ! 48 & %floor% ! 49 & %floor% < 96] AddFillerWalls ConnectionWall, 0.5, %CtrX% - %DimX% + 0.5, %CtrZ% - 2.5, 3, 7, 0, false, 0, 0, false
		if[%floor% ! 0 & %floor% ! 1 & %floor% ! 48 & %floor% ! 49 & %floor% < 96] AddFillerWalls ConnectionWall, 0.5, %CtrX% - %DimX% + 0.5, %CtrZ% + 2.5, 3, 7, 0, false, 0, 0, false

		#East doors
		WallOrientation = left
		TextureOverride %DTex%, %DTex%, %DSTex%, %DSTex%, %DSTex%, %DSTex%
		AddDoor , , false, %DTex%, 0.2, 4, 75, %CtrX% + %DimX% - 0.5, %CtrZ% - 2.5, 3, 7, 0, 0, 0
		TextureOverride %DTex%, %DTex%, %DSTex%, %DSTex%, %DSTex%, %DSTex%
		AddDoor , , false, %DTex%, 0.2, 1, 75, %CtrX% + %DimX% - 0.5, %CtrZ% + 2.5, 3, 7, 0, 0, 0

		AddFillerWalls ConnectionWall, 0.5, %CtrX% + %DimX% - 0.5, %CtrZ% - 2.5, 3, 7, 0, false, 0, 0, false
		AddFillerWalls ConnectionWall, 0.5, %CtrX% + %DimX% - 0.5, %CtrZ% + 2.5, 3, 7, 0, false, 0, 0, false

	#Restroom signs
	Set BoxTex = WoodSign2
	if[%floor% = 48 | %floor% = 49] Set BoxTex = BlackTrim
	Set SignSuffix = _Brown
	if[%floor% = 48 | %floor% = 49] Set SignSuffix = _Gray

		#West
		if[%floor% ! 0 & %floor% ! 1 & %floor% ! 48 & %floor% ! 49 & %floor% < 96] AddBoxSign(%RName%_SignWest1, %Sign1%%SignSuffix%, %Sign1%%SignSuffix%, %BoxTex%, 0.05, true, true, true, %CtrX% - %DimX% - 0.75, %CtrZ% - 2.5, 1.5, 1.5, 8, false)
		if[%floor% ! 0 & %floor% ! 1 & %floor% ! 48 & %floor% ! 49 & %floor% < 96] AddBoxSign(%RName%_SignWest2, %Sign2%%SignSuffix%, %Sign2%%SignSuffix%, %BoxTex%, 0.05, true, true, true, %CtrX% - %DimX% - 0.75, %CtrZ% + 2.5, 1.5, 1.5, 8, false)

		#East
		AddBoxSign(%RName%_SignEast1, %Sign1%%SignSuffix%, %Sign1%%SignSuffix%, %BoxTex%, 0.05, true, true, true, %CtrX% + %DimX% + 0.75, %CtrZ% - 2.5, 1.5, 1.5, 8, false)
		AddBoxSign(%RName%_SignEast2, %Sign2%%SignSuffix%, %Sign2%%SignSuffix%, %BoxTex%, 0.05, true, true, true, %CtrX% + %DimX% + 0.75, %CtrZ% + 2.5, 1.5, 1.5, 8, false)
<EndFunction>

