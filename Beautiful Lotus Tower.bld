	Set WPanelHeight = 8.5 - 0.1 - 0.5
	Set Direction = %param2%

		#Front panels
		if[%Direction% = 1] CreateWallBox2 elevator, Panel_F1, %WPTex%, 0 - (7.1 * 1/3), -3.05 + 0.01, (7.1 * 1/3) - 0.3, 0.02, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		if[%Direction% = 1] CreateWallBox2 elevator, Panel_F2, %WPTex%, 0, -3.05 + 0.01, (7.1 * 1/3) - 0.3, 0.02, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		if[%Direction% = 1] CreateWallBox2 elevator, Panel_F3, %WPTex%, 0 + (7.1 * 1/3), -3.05 + 0.01, (7.1 * 1/3) - 0.3, 0.02, %WPanelHeight%, 0.25, 0, 0, false, true, true, true

		#Back panels
		if[%Direction% = 2] CreateWallBox2 elevator, Panel_B1, %WPTex%, 0 - (7.1 * 1/3), 3.05 - 0.01, (7.1 * 1/3) - 0.3, 0.02, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
