<Function ShaftDoors_Office>
	Set StartNo = %param1% #Number of the first elevator
	Set NP = %param2% #Name prefix
	Set S1 = %param3% #1st shaft no.
	Set S2 = %param4% #2nd shaft no.

	#Shaft doors
	Set IDP-ShaftDoors = 3.25 + 0.3 #IDP = Initial door position
		WallOrientation = left
		AddShaftDoorComponent %StartNo%, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, right, 0.8, 0.6, -2 + (2 * 1), %IDP-ShaftDoors%, -2 + (2 * 2), %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent %StartNo%, 1, Right, %DoorTexture%2, %DoorTexture%2, 0.2, left, 0.8, 0.6, -2, %IDP-ShaftDoors%, -2 + (2 * 1), %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo%, 1, %AutoFiller%, false

		WallOrientation = left
		AddShaftDoorComponent %StartNo% + 1, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, right, 0.8, 0.6, -2 + (2 * 1), %IDP-ShaftDoors%, -2 + (2 * 2), %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent %StartNo% + 1, 1, Right, %DoorTexture%2, %DoorTexture%2, 0.2, left, 0.8, 0.6, -2, %IDP-ShaftDoors%, -2 + (2 * 1), %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo% + 1, 1, %AutoFiller%, false

		WallOrientation = left
		AddShaftDoorComponent %StartNo% + 2, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, right, 0.8, 0.6, -2 + (2 * 1), %IDP-ShaftDoors%, -2 + (2 * 2), %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent %StartNo% + 2, 1, Right, %DoorTexture%2, %DoorTexture%2, 0.2, left, 0.8, 0.6, -2, %IDP-ShaftDoors%, -2 + (2 * 1), %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo% + 2, 1, %AutoFiller%, false

	Set IDP-ShaftDoors = -3.25 - 0.3
		WallOrientation = right
		AddShaftDoorComponent %StartNo% + 3, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, left, 0.8, 0.6, 2 - (2 * 2), %IDP-ShaftDoors%, 2 - (2 * 1), %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent %StartNo% + 3, 1, Right, %DoorTexture%2, %DoorTexture%2, 0.2, right, 0.8, 0.6, 2 - (2 * 1), %IDP-ShaftDoors%, 2, %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo% + 3, 1, %AutoFiller%, false

		WallOrientation = right
		AddShaftDoorComponent %StartNo% + 4, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, left, 0.8, 0.6, 2 - (2 * 2), %IDP-ShaftDoors%, 2 - (2 * 1), %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent %StartNo% + 4, 1, Right, %DoorTexture%2, %DoorTexture%2, 0.2, right, 0.8, 0.6, 2 - (2 * 1), %IDP-ShaftDoors%, 2, %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo% + 4, 1, %AutoFiller%, false

		WallOrientation = right
		AddShaftDoorComponent %StartNo% + 5, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, left, 0.8, 0.6, 2 - (2 * 2), %IDP-ShaftDoors%, 2 - (2 * 1), %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		AddShaftDoorComponent %StartNo% + 5, 1, Right, %DoorTexture%2, %DoorTexture%2, 0.2, right, 0.8, 0.6, 2 - (2 * 1), %IDP-ShaftDoors%, 2, %IDP-ShaftDoors%, 7, 0, 0, 0, 0, 0
		FinishShaftDoor %StartNo% + 5, 1, %AutoFiller%, false

	#Door tracks
	AddShaftTrack(%NP%1, %S1%, -10 - 4, -10 + 4, 4.67 - 0.53, 4.67 - 0.33, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)
	AddShaftTrack(%NP%2, %S1%, 0 - 4, 0 + 4, 4.67 - 0.53, 4.67 - 0.33, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)
	AddShaftTrack(%NP%3, %S1%, 10 - 4, 10 + 4, 4.67 - 0.53, 4.67 - 0.33, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)
	AddShaftTrack(%NP%4, %S2%, -10 - 4, -10 + 4, -4.67 + 0.33, -4.67 + 0.53, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)
	AddShaftTrack(%NP%5, %S2%, 0 - 4, 0 + 4, -4.67 + 0.33, -4.67 + 0.53, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)
	AddShaftTrack(%NP%6, %S2%, 10 - 4, 10 + 4, -4.67 + 0.33, -4.67 + 0.53, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)
<EndFunction>

<Function Z1_EWP>
	#Panels on walls for Zone 1 elevators
