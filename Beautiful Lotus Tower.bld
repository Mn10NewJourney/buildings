#HALL STATIONS (PASSENGER)
#Set textures
<Function ElevHallTex>
	#Shaft doors
	if[%floor% < -2] Set DoorTexture = PLElevDoors
	if[%floor% = -2 | %floor% = -1] Set DoorTexture = LLElevDoors
	if[%floor% = 0 | %floor% = 1] Set DoorTexture = LobbyElevDoors
	if[%floor% > 1 & %floor% < 45] Set DoorTexture = OLElevDoors
	if[%floor% = 48 | %floor% = 49] Set DoorTexture = SkylobbyElevDoors
	if[%floor% > 49 & %floor% < 96] Set DoorTexture = UOLElevDoors
	if[%floor% > 97 & %floor% < 103] Set DoorTexture = SummitElevDoors

	#Use automatic filler walls or not
	Set AutoFiller = false

	#Door trims
		#Sides
		Set DoorTrimTexture = %DoorTexture%_Trim

		#Top
		Set DoorTrimTexture_Top = %DoorTrimTexture%
		Set DoorTrimTexture_TopAlt = %DoorTrimTexture%
		if[%floor% = 48 | %floor% = 49] Set DoorTrimTexture_TopAlt = %DoorTexture%_TrimTop #Used for Skylobby Mezzanine Elevator

	#Deco dimensions
		Set SideDecoH = 7.2
		Set TopDecoH = 0.2
		if[%floor% = 0] Set SideDecoH = %height% + Floor(1).FullHeight
		if[%floor% = 0] Set TopDecoH = %SideDecoH% - 7
		if[%floor% = 48] Set SideDecoH = %height% + Floor(49).FullHeight
		if[%floor% = 48] Set TopDecoH = %SideDecoH% - 7

		#if[%floor% > -3 & %floor% < 0] Set SideDecoH = 8
		#if[%floor% > -3 & %floor% < 0] Set TopDecoH = 1
		if[%floor% < 0] Set SideDecoH = 8
		if[%floor% < 0] Set TopDecoH = 1

		if[%floor% = 1 | %floor% > 97] Set SideDecoH = %height%
		if[%floor% = 1 | %floor% > 97] Set TopDecoH = %height% - 7

	#Deco dimensions (Parking Elevators)
		Set SideDecoH-Alt = %SideDecoH%
		Set TopDecoH-Alt = %TopDecoH%
		if[%floor% = 0] Set SideDecoH-Alt = %height%
		if[%floor% = 0] Set TopDecoH-Alt = %SideDecoH-Alt% - 7

	#Deco dimensions (Skylobby Mezzanine Elevator)
		Set SideDecoH-Alt2 = %SideDecoH%
		Set TopDecoH-Alt2 = %TopDecoH%
		if[%floor% = 48 | %floor% = 49] Set SideDecoH-Alt2 = %height%
		if[%floor% = 48 | %floor% = 49] Set TopDecoH-Alt2 = %SideDecoH-Alt2% - 7

	#Call button panels
	Set BPanelTexture = ElevExtPanels
	if[%floor% > -1 & %floor% < 45] Set BPanelTexture = ElevExtPanels2
	Set ButtonPrefix = T2Button

	#Lantern panels
	Set LPanelTexture = %BPanelTexture%
	Set LanternPrefix = T2Lantern

		#Lateral offset for lantern panels
		Set ExtraLateralOffset = 0
		#if[(%floor% > -3 & %floor% < 2) | %floor% > 97] Set ExtraLateralOffset = 0.05
		if[%floor% < 2 | %floor% > 97] Set ExtraLateralOffset = 0.05
		if[%floor% = 48 | %floor% = 49] Set ExtraLateralOffset = 0.05
<EndFunction>

