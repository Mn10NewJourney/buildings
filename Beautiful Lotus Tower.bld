		if[%Direction% = 2] CreateWallBox2 elevator, Panel_B3, %WPTex%, 0 + (7.1 * 1/3), 3.05 - 0.01, (7.1 * 1/3) - 0.3, 0.02, %WPanelHeight%, 0.25, 0, 0, false, true, true, true

		#Left panels
		CreateWallBox2 elevator, Panel_L1, %WPTex%, -3.55 + 0.01, 0 - (6.1 * 1/3), 0.02, (6.1 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_L2, %WPTex%, -3.55 + 0.01, 0, 0.02, (6.1 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_L3, %WPTex%, -3.55 + 0.01, 0 + (6.1 * 1/3), 0.02, (6.1 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true

		#Right panels
		CreateWallBox2 elevator, Panel_R1, %WPTex%, 3.55 - 0.01, 0 - (6.1 * 1/3), 0.02, (6.1 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_R2, %WPTex%, 3.55 - 0.01, 0, 0.02, (6.1 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_R3, %WPTex%, 3.55 - 0.01, 0 + (6.1 * 1/3), 0.02, (6.1 * 1/3) - 0.3, %WPanelHeight%, 0.25, 0, 0, false, true, true, true
<EndFunction>

<Function Z2_EWP>
	#Panels on walls for Zone 2 elevators
	Set WPTex = %param1%
	#Set WPanelHeight = 8.5 - 0.1 - 0.5
