#Set textures
<Function FloorTex>
	if[%floor% < -2] Set 5 = Road
	if[%floor% = -2 | %floor% = -1] Set 5 = LowerLevelFloor
	if[%floor% = 0 | %floor% = 1] Set 5 = LobbyFloor
	if[%floor% > 1 & %floor% < 45] Set 5 = OfficeFloor
	if[%floor% > 44 & %floor% < 48] Set 5 = ServiceFloor
	if[%floor% = 48] Set 5 = SkylobbyFloor
	if[%floor% = 49] Set 5 = SkylobbyFloor2
	if[%floor% > 49 & %floor% < 96] Set 5 = OfficeFloor2
	if[%floor% > 95 & %floor% < 98] Set 5 = ServiceFloor2
	if[%floor% > 97 & %floor% < 103] Set 5 = SummitFloor
	if[%floor% > 102 & %floor% < 109] Set 5 = ServiceFloor2
	if[%floor% = 109] Set 5 = RoofFloor
<EndFunction>

<Function WallTex>
	if[%floor% < -2] Set 6 = ParkingWall
	if[%floor% = -2 | %floor% = -1] Set 6 = LowerLevelWall
	if[%floor% = 0 | %floor% = 1] Set 6 = LobbyWall
	if[%floor% > 1 & %floor% < 45] Set 6 = OfficeWall
	if[%floor% > 44 & %floor% < 48] Set 6 = ServiceWall
	if[%floor% = 48 | %floor% = 49] Set 6 = SkylobbyWall
	if[%floor% > 49 & %floor% < 96] Set 6 = OfficeWall2
	if[%floor% > 95 & %floor% < 98] Set 6 = ServiceWall2
	if[%floor% > 97 & %floor% < 103] Set 6 = SummitWall
	if[%floor% > 102 & %floor% < 109] Set 6 = ServiceWall2
	if[%floor% = 109] Set 6 = ExtFacade

		#Override for elevator lobby
		Set 6Lobby = %6%
		if[%floor% > -11 & %floor% < -2] Set 6Lobby = ParkingLobbyWall
		
		#Service elevator lobby
		Set 6S = %6%
		if[%floor% > -3 & %floor% < 45] Set 6S = ServiceLobbyWall
		if[%floor% > 47 & %floor% < 96] Set 6S = ServiceLobbyWall2
		if[%floor% > 97 & %floor% < 103] Set 6S = ServiceLobbyWall2

		#Parking ramps
		Set 6Ramp = ParkingWall4 #Walls around ramp

		#Shops
		Set 6SH = %6%
		if[%floor% = -2 | %floor% = -1] Set 6SH = ShopsWall
		
		#Escalator railings
		Set 6ER = %6%
		if[%floor% = -2 | %floor% = -1] Set 6ER = LowerLevelRail
		if[%floor% = 0 | %floor% = 1] Set 6ER = LobbyRail

		#Railings on mechanical floors and 99th-100th floors
		Set 6R = %6%
		if[%floor% = 45 | %floor% = 46 | %floor% = 47 | %floor% = 108] Set 6R = ServiceRailing
		if[%floor% = 99 | %floor% = 100] Set 6R = SummitWall2

		#Override for support columns
		Set 6CL = %6%
		if[%floor% < 0] Set 6CL = LowerLevelWall2

	#Set Heights for External Shaft/Stairwell walls
	Set SHeight = %height%
	Set SOffset = 0

	if[%floor% > -10 & %floor% < -3] Set SHeight = %fullheight%
	if[%floor% > -10 & %floor% < -3] Set SOffset = 0
	if[%floor% = -11] Set SHeight = %fullheight% + Floor(-10).InterfloorHeight - 0.5
	if[%floor% = -11] Set SOffset = 0
	if[%floor% = -10] Set SHeight = %height%
	if[%floor% = -10] Set SOffset = %interfloorheight%
	if[%floor% = -3] Set SHeight = %fullheight% + Floor(-2).InterfloorHeight - 0.5
	if[%floor% = -3] Set SOffset = 0

	if[%floor% = 1 | %floor% = 49 | %floor% = 100] Set SHeight = %fullheight%
	if[%floor% = 1 | %floor% = 49 | %floor% = 100] Set SOffset = -%interfloorheight%
	if[%floor% = 46 | %floor% = 47 | %floor% = 108] Set SHeight = %fullheight%
	if[%floor% = 46 | %floor% = 47 | %floor% = 108] Set SOffset = -%interfloorheight%
	if[%floor% = 109] Set SHeight = %height%
	if[%floor% = 109] Set SOffset = %interfloorheight%

	Set IsExternal = false
	if[%floor% < -2 | %floor% = 109] Set IsExternal = true
	
	Set ShowTop = false
	if[%floor% = 109] Set ShowTop = true
<EndFunction>

