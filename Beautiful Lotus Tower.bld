#Restrooms for all zones
<Function RDTex> #RD = Restroom Doors
	Set DoorTx = Door9b
	if[%floor% = -2 | %floor% = -1 | %floor% = 0 | %floor% = 1] Set DoorTx = Door8
	if[%floor% = 48 | %floor% = 49] Set DoorTx = Door6
