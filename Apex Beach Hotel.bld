<Function BridgeElev-RearDoor>
	#Front Shaft Doors
	WallOrientation = left
	AddShaftDoorComponent 1, 2, 2Speed_Fast, %DoorTexture%2, %DoorTexture%2, 0.18, left, 0.8, 0.6, 0.5, 3.5 + 0.48, 0.5 + 2, 3.5 + 0.48, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent 1, 2, 2Speed_Slow, %DoorTexture%2, %DoorTexture%2, 0.18, left, 0.4, 0.3, 0.5 - 2, 3.5 + 0.66, 0.5, 3.5 + 0.66, 7, 0, 0, 0, 0, 0
	FinishShaftDoor 1, 2, false, false

		#Gap filler
		WallOrientation = right
		DrawWalls = true, true, true, true, true, true
		TextureOverride ElevExt, ElevExt, %DoorTexture%_Trim, ElevExt, ElevExt, ElevExt
		AddShaftWall 1, RearGapFiller, ElevExt, 0.36, 1 + 2, 4.67 - 0.33, 1 + 2 + 0.05, 4.67 - 0.33, 7, 7, %interfloorheight%, %interfloorheight%, 0, 0
	
		#Door tracks
		AddDoorTrack(1, 1, 1 - 4, 1 + 2, 4.67 - 0.33 - 0.36, 4.67 - 0.33, ElevDoorTrackZ)
	
		#Top filler
		AddTopFiller(1, 1, 1, 1 + 2, 4.67 - 0.33 - 0.18, 4.67 - 0.33, 7, %DoorTexture%_Trim)

	#Door trims
	AddDeco4(%DoorTrimTexture%, %DoorTrimTexture_Top%, back, true, false, external, 0.05, 0.2, -97 + 1, 295, 4, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66)

	#Call buttons
	CallButtonElevators = 1
	CreateCallButtons %BPanelTexture%, ButtonUp, ButtonUpLit, ButtonDown, ButtonDownLit, -98 - 1.25, 295 + 0.001, 4 - 0.4, back, 0.4, 0.8, true, 0, 0

	#Indicators
	#AddDirectionalIndicator 1, false, false, false, true, %LPanelTexture%, LanternUp, LanternUpLit, LanternDown, LanternDownLit, -98 - 1.25, 295 + 0.001, 5.5, back, 0.5, 1, true, 0, 0
		Set Offset = 8
		Set ExtraLateralOffset = 0.05
		ACMELobbyInds(back, 1, -97 + 1, 295 + %ExtraLateralOffset%, %Offset%, 0, 2)

	#Signs
		DrawWalls = false, true, false, false, false, false
		AddWall E1HallSign, ElevHallSign, 0, -99.25 - 0.375, 295 + 0.001, -99.25 + 0.375, 295 + 0.001, 1, 1, 2.4 + %interfloorheight%, 2.4 + %interfloorheight%, 0, 0, true
<EndFunction>

<Floors 0 to 2>
	#Set hall textures
		#Shaft door texture
		Set DoorTexture = BridgeElevShaftDoors
		if[%floor% = 1] Set DoorTexture = BLElevDoors
	
		#Door trims
			#Sides
			Set DoorTrimTexture = %DoorTexture%_Trim
	
			#Top
			Set DoorTrimTexture_Top = %DoorTrimTexture%
	
		#Deco dimensions
			Set SideDecoH = 7.2
			Set TopDecoH = 0.2
			if[%floor% = 1] Set SideDecoH = %height% + Floor(%floor% + 1).FullHeight
			if[%floor% = 1] Set TopDecoH = %height% + Floor(%floor% + 1).FullHeight - 7
		
		#Call button panels
		Set BPanelTexture = ElevExtPanels
	
		#Lantern panels
		Set LPanelTexture = %BPanelTexture%

	#Set textures for ACME Floor Indicator function
	ACMEInd_LanternTex(LanternUp, LanternUpLit, LanternDown, LanternDownLit, %LPanelTexture%, Ind_Button)

	#Create hall components
	if[%floor% ! 1] BridgeElev-FrontDoor()
	if[%floor% = 1] BridgeElev-RearDoor()
