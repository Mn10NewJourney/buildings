#Service elevators
<Function SE_HallTx>
	#Shaft doors
	Set SDoorTexture = ElevDoorsB
	if[%floor% = 0] Set SDoorTexture = ElevDoorsA
	if[%floor% = 13] Set SDoorTexture = ElevDoorsC

	#Use automatic filler walls or not
	Set AutoFiller = false

	#Door trims
		#Sides
		Set SDoorTrimTexture = %SDoorTexture%_Trim

		#Top
		Set SDoorTrimTexture_Top = %SDoorTrimTexture%

	#Deco dimensions
		Set SSideDecoH = 7.2
		Set STopDecoH = 0.2

	#Call button panels
	Set SBPanelTexture = ElevExtPanels

	#Lantern panels
	Set SLPanelTexture = %SBPanelTexture%
<EndFunction>

