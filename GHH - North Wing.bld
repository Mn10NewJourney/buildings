		#Lantern heights
		Set LHeight = 1.5
		if[%floor% = 9] Set LHeight = 0.8
		Set LOffset = 6 - (%LHeight% / 2)

		#Lateral offset for lantern panels
		Set ExtraLateralOffset = 0
