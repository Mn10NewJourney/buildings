#Set textures
<Function FloorTex>
	if[%floor% = -1] Set 5 = LowerLobbyFloor
	if[%floor% = 0] Set 5 = LobbyFloor2
	if[%floor% = 1 | %floor% = 2] Set 5 = LobbyFloor
	if[%floor% = 3] Set 5 = FCFloor
	if[%floor% > 3 & %floor% < 17] Set 5 = GuestroomFloor
	if[%floor% = 17] Set 5 = SummitFloor
	if[%floor% = 18] Set 5 = ServiceFloor
	Set 5B = RoofFloor #Roof
	
		#Elevator machinery enclosures (Roof level)
		Set 5Q = %5%
		if[%floor% = 19] Set 5Q = ElevMRIntFloor
	
		#Override for additional textures
		Set 5D = %5%
		if[%floor% < 3] Set 5D = LobbyCarpet
		if[%floor% = 3] Set 5D = FCCarpet1
		if[%floor% > 3 & %floor% < 17] Set 5D = GuestroomFloor2
<EndFunction>

<Function WallTex>
	if[%floor% = -1] Set 6 = LowerLobbyWall
	if[%floor% > -1 & %floor% < 3] Set 6 = LobbyWall
	if[%floor% > 3 & %floor% < 17] Set 6 = GuestroomWall
	if[%floor% = 3] Set 6 = FCWall
	if[%floor% = 17] Set 6 = SummitWall
	if[%floor% = 18] Set 6 = ServiceWall
	if[%floor% = 19] Set 6 = ExtFacade

		#Elevator machinery enclosures (Roof level)
		Set 6Q = %6%
		if[%floor% = 19] Set 6Q = ElevMRIntWall

		#Service elevator lobby
		Set 6S = %6%
		if[%floor% < 4 | %floor% = 17] Set 6S = ServiceLobbyWall
		if[%floor% > 3 & %floor% < 17] Set 6S = ServiceLobbyWall2
		if[%floor% = 19] Set 6S = ServiceWall
<EndFunction>

<Function CeilingTex>
	if[%floor% = -1] Set 7 = LowerLobbyCeiling
	if[%floor% = 0] Set 7 = LobbyCeiling2
	if[%floor% = 1 | %floor% = 2] Set 7 = LobbyCeiling
	if[%floor% = 3] Set 7 = FCCeiling
	if[%floor% > 3 & %floor% < 17] Set 7 = GuestroomCeiling
	if[%floor% = 17] Set 7 = SummitCeiling
	if[%floor% = 18] Set 7 = ServiceCeiling
	
		#Elevator machinery enclosures (Roof level)
		Set 7Q = %7%
		if[%floor% = 19] Set 7Q = ElevMRIntCeiling
<EndFunction>

