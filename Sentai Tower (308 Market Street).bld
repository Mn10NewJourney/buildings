		if[%floor% < 11] CreateShaftWall(%5%Wall, %5%Wall, %5%%WallSuffix%, %ExtTex%, %ExtTex%, %5%Wall, 0.5, %21%, %22%, %23% + 0.5, %24% + 0.5, ST%25%, %SWHeight%, %SWOffset%, true, true, true, true, %ShowTop%, false, true)
		if[%floor% = 11] CreateShaftWall(%ExtTex%, %ExtTex%, %ExtTex%, %ExtTex%, %ExtTex%, %5%Wall, 0.5, %21%, %22%, %23% + 0.5, %24% + 0.5, ST%25%, %interfloorheight% + 9.5, %SWOffset%, true, true, true, true, %ShowTop%, false, true)

	#Doors
	WallOrientation = right
	TextureOverride ServiceDoor2B, ServiceDoor2, ServiceDoor2Side, ServiceDoor2Side, ServiceDoor2Side, ServiceDoor2Side
	if[%floor% ! 1 & %floor% ! 10] AddStairsDoor %25%, , , false, ServiceDoor2, 0.2, 6, 75, %23% - 2, -%24%, 3, 7, 0, 0, 0
	WallOrientation = left
