#Set textures
<Function TexPrefix>
	#Main prefixes
	if[%floor% > 2 & %floor% < 8] Set 5 = Parking
	if[%floor% = 1 | %floor% = 2 | %floor% = 8] Set 5 = Shop
	if[%floor% = 9 | %floor% = 10] Set 5 = F9
	if[%floor% = 11] Set 5 = F10
	if[%floor% > 11 & %floor% < 16] Set 5 = F11
	if[%floor% > 15 & %floor% < 26] Set 5 = Office
	if[%floor% = 26] Set 5 = Service

	#Override for 1st floor office lobby
	Set 5A = %5%
	if[%floor% = 1] Set 5A = OFLobby

	#Override for 1st floor office lobby (Support columns)
	Set 5C = %5%
	if[%floor% = 1] Set 5C = OFLobbySupport

	#Service elevator lobby
	Set 5S = %5%
	if[%floor% = 1 | %floor% = 2] Set 5S = ServiceLobby
	if[%floor% > 7 & %floor% < 26] Set 5S = ServiceLobby

	Set 5SE = %5%
	if[%floor% = 1] Set 5SE = %5A%
	if[%floor% = 2] Set 5SE = ServiceLobby

	#Service elevator's shaft doors
	Set 5E = ServiceA
	if[%floor% > 2 & %floor% < 8] Set 5E = ServiceB
	if[%floor% = 26] Set 5E = ServiceB
<EndFunction>

