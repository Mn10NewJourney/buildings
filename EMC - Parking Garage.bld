#Set textures for Elevators 7-8
<Function ElevHallTex_E7-8>
	#Shaft doors
	TexPrefix() #Set prefixes for shaft doors
	Set DoorTexture = %E7SD%ShaftDoors

	#Use automatic filler walls or not
	Set AutoFiller = false

	#Door trims
		#Sides
		Set DoorTrimTexture = %DoorTexture%_Trim

		#Top
		Set DoorTrimTexture_Top = %DoorTrimTexture%

	#Deco dimensions
		Set SideDecoH = 7.2
		Set TopDecoH = 0.2

	#Call button panels
	Set BPanelTexture = ElevExtPanels
	Set ButtonPrefix = TKVRButton

	#Lantern panels
	Set LPanelTexture = %BPanelTexture%
	Set LanternPrefix = TKVRLantern

		#Lateral offset for lantern panels
		Set ExtraLateralOffset = 0
<EndFunction>

<Function E7-8FillerWalls>
	Set ElevNo = %param1%
	Set CenterX = %param2%

	#Manual wall cuts for external mesh
	CutAll %CenterX% - 2, %interfloorheight%, %Shaft1CZ% - 5 - 0.1, %CenterX% + 2, %interfloorheight% + 4, %Shaft1CZ% - 5 + 0.1, true, false

	#Upper filler walls
	WallOrientation = left
	AddFillerWalls ALobbyWall, 0.66, %CenterX%, %Shaft1CZ% - 5, 4, 7 - 4, 4 + %interfloorheight%, true, 0, 0, true

		#Lower filler walls to match with shaft walls
		DrawWalls = true, true, false, false, false, false
		AddWall E%Elevator%Filler1, ALobbyWall_Lower, 0, %CenterX% - 2, %Shaft1CZ% - 5, %CenterX% - 2, %Shaft1CZ% - 5 + 0.66, 4, 4, %interfloorheight%, %interfloorheight%, 0, 0, true
		AddWall E%Elevator%Filler2, ALobbyWall_Lower, 0, %CenterX% + 2, %Shaft1CZ% - 5, %CenterX% + 2, %Shaft1CZ% - 5 + 0.66, 4, 4, %interfloorheight%, %interfloorheight%, 0, 0, true
<EndFunction>

#Additional Elevators - No. 7-8
<Floors -1 to 8>
	#Shaft Doors
	ElevHallTex_E7-8() #Set shaft door and panel textures
	Set StartNo = 7 #Number of the first elevator

	Set IDP = -3.25 - 0.3

	WallOrientation = right
	AddShaftDoorComponent %StartNo%, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, left, 0.8, 0.6, -2, %IDP%, 0, %IDP%, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo%, 1, Right, %DoorTexture%2, %DoorTexture%2, 0.2, right, 0.8, 0.6, 0, %IDP%, 2, %IDP%, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo%, 1, %AutoFiller%, false

	WallOrientation = right
	AddShaftDoorComponent %StartNo% + 1, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, left, 0.8, 0.6, -2, %IDP%, 0, %IDP%, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo% + 1, 1, Right, %DoorTexture%2, %DoorTexture%2, 0.2, right, 0.8, 0.6, 0, %IDP%, 2, %IDP%, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo% + 1, 1, %AutoFiller%, false

	#Door tracks
	AddShaftTrack(7, 3, -5 - 4, -5 + 4, -4.34, -4.14, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)
	AddShaftTrack(8, 3, 5 - 4, 5 + 4, -4.34, -4.14, ElevDoorTrackZ, ElevExt, 0.05, 1, 7)

	#****************************************#

	#Door trims/fillers
	WallTex() #Set filler textures to match with shaft walls
	Set Shaft1CX = -28 #1st shaft for elevator bank
	Set Shaft1CZ = 104.5

		#All floors
		E7-8FillerWalls(7, %Shaft1CX% - 5)
		E7-8FillerWalls(8, %Shaft1CX% + 5)

	#****************************************#

	#Call buttons
	CallButtonElevators = 7, 8
	CreateCallButtons %BPanelTexture%, %ButtonPrefix%Up, %ButtonPrefix%UpLit, %ButtonPrefix%Down, %ButtonPrefix%DownLit, %Shaft1CX%, %Shaft1CZ% - 5.001, 3.5, front, 0.5, 1, true, 0, 0

		#Signs
		Set Suffix = %floorid%

		DrawWalls = true, false, false, false, false, false
		AddWall E7-8FloorSign, BlankSign%Suffix%, 0, %Shaft1CX% - 0.75, %Shaft1CZ% - 5.001, %Shaft1CX% + 0.75, %Shaft1CZ% - 5.001, 1.5, 1.5, 5.25, 5.25, 0, 0, false
		AddWall E7-8HallSign, FireSign, 0, %Shaft1CX% - 0.25, %Shaft1CZ% - 5.001, %Shaft1CX% + 0.25, %Shaft1CZ% - 5.001, 0.75, 0.75, 2.6, 2.6, 0, 0, false

	#****************************************#

	#Indicator sets
	Set Offset = 7.5
	if[%floor% = 8] Set Offset = 7.75

		#Set textures for Elevators 7-8
		ACMEInd_LanternTex(%LanternPrefix%Up, %LanternPrefix%UpLit, %LanternPrefix%Down, %LanternPrefix%DownLit, %LPanelTexture%, TKVR_OInd_Button)

		#Indicator creation
		ACMELobbyInds(front, 7, %Shaft1CX% - 5, %Shaft1CZ% - 5, %Offset%, -1, 8, true)
		ACMELobbyInds(front, 8, %Shaft1CX% + 5, %Shaft1CZ% - 5, %Offset%, -1, 8, true)

	#****************************************#

	#Elevator number signs
	DrawWalls = true, false, false, false, false, false
	AddWall E7Sign, ElevNo7, 0, (%Shaft1CX% - 5 - 1.25) - 0.25, %Shaft1CZ% - 5 - %ExtraLateralOffset% - 0.001, (%Shaft1CX% - 5 - 1.25) + 0.25, %Shaft1CZ% - 5 - %ExtraLateralOffset% - 0.001, 0.5, 0.5, %Offset%, %Offset%, 0, 0, false
	AddWall E8Sign, ElevNo8, 0, (%Shaft1CX% + 5 - 1.25) - 0.25, %Shaft1CZ% - 5 - %ExtraLateralOffset% - 0.001, (%Shaft1CX% + 5 - 1.25) + 0.25, %Shaft1CZ% - 5 - %ExtraLateralOffset% - 0.001, 0.5, 0.5, %Offset%, %Offset%, 0, 0, false
<EndFloors>

