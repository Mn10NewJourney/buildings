<Floors -1 to 33>
	Set 8 = 21 #Center X
	Set 9 = -39 #Center Z
	Set 10 = 5.67 #Cut X
	Set 11 = 23.67 #Cut Z
	Set 12 = 8 #Shaft Number
	
	FloorOrientation = bottom
	DrawWalls = false, true, false, false, false, false
	if[%floor% = -1] AddShaftFloor %12%, Shaft%12%Base, Concrete1, 0.5, -%10%, -%11%, %10%, %11%, 0, 0, 0, 0
	FloorOrientation = top
	DrawWalls = true, true, false, false, false, false
	if[%floor% = 33] AddShaftFloor %12%, MachineRoomBase, Concrete1, 0.5, -%10%, -%11%, %10% - 0.33, %11%, %interfloorheight%, %interfloorheight%, 0, 0
	DrawWalls = true, false, false, false, false, false
	if[%floor% = 33] AddShaftFloor %12%, Shaft%12%Top, Concrete1, 0.5, -%10%, -%11%, %10%, %11%, %fullheight%, %fullheight%, 0, 0
	
	WallOrientation = Left
	DrawWalls = true, true, false, false, false, false
	AddShaftWall %12%, Shaft%12%Front, Concrete2, 0.33, -%10%, -%11%, %10%, -%11%, %fullheight%, %fullheight%, 0, 0, 0, 0
	AddShaftWall %12%, Shaft%12%Left, Concrete2, 0.33, -%10%, -%11%, -%10%, %11%, %fullheight%, %fullheight%, 0, 0, 0, 0
	WallOrientation = Right
	AddShaftWall %12%, Shaft%12%Right, Concrete2, 0.33, %10%, -%11%, %10%, %11%, %fullheight%, %fullheight%, 0, 0, 0, 0
	AddShaftWall %12%, Shaft%12%Back, Concrete2, 0.33, -%10%, %11%, %10%, %11%, %fullheight%, %fullheight%, 0, 0, 0, 0
	
	#External Shaft Walls
	Set 13 = OtherWall #External Shaft Walls
	if[%floor% < 13] Set 13 = LobbyWall #External Shaft Walls
	Set 14 = %height% #External Shaft Walls Height
	Set 15 = 0 #External Shaft Walls Offset
	if[%floor% > 0 & %floor% < 13] Set 14 = %fullheight% #External Shaft Walls Height
	if[%floor% > 0 & %floor% < 13] Set 15 = -%interfloorheight% #External Shaft Walls Offset
	CreateShaftWall(%13%, %13%, %13%, %13%, %13%, %13%, 0.33, 21, -39, 6, 24, %12%, %14%, %15%, true, true, true, true, false, false, false)

	#Machine Room Doors
	WallOrientation = Center
	if[%floor% = 33] AddDoor , , false, Door1, 0.2, 3, 75, 21 + 5.67, -39 + 1.75, 3.5, 8, 0, 0, 0
	if[%floor% = 33] AddDoor , , false, Door1, 0.2, 2, 75, 21 + 5.67, -39 - 1.75, 3.5, 8, 0, 0, 0
