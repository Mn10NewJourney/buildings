#Shaft door functions
<Function ShaftDoors_Right>
	Set StartNo = %param1%
	Set TexturePrefix = %param2%

	WallOrientation = left
	AddShaftDoorComponent %StartNo%, 1, Left, %TexturePrefix%, %TexturePrefix%, 0.2, left, 0.8, 3.8, -2.25, 3.8, 0, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo%, 1, Right, (%TexturePrefix%)2, (%TexturePrefix%)2, 0.2, right, 0.8, 3.8, 0, 3.8, 2.25, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo%, 1, false, false

	AddShaftDoorComponent %StartNo% + 1, 1, Left, %TexturePrefix%, %TexturePrefix%, 0.2, left, 0.8, 3.8, -2.25, 3.8, 0, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo% + 1, 1, Right, (%TexturePrefix%)2, (%TexturePrefix%)2, 0.2, right, 0.8, 3.8, 0, 3.8, 2.25, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo% + 1, 1, false, false

	AddShaftDoorComponent %StartNo% + 2, 1, Left, %TexturePrefix%, %TexturePrefix%, 0.2, left, 0.8, 3.8, -2.25, 3.8, 0, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo% + 2, 1, Right, (%TexturePrefix%)2, (%TexturePrefix%)2, 0.2, right, 0.8, 3.8, 0, 3.8, 2.25, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo% + 2, 1, false, false
<EndFunction>

<Function ShaftDoors_Left>
	Set StartNo = %param1%
	Set TexturePrefix = %param2%

	WallOrientation = right
	AddShaftDoorComponent %StartNo%, 1, Left, %TexturePrefix%, %TexturePrefix%, 0.2, right, 0.8, -3.8, 0, -3.8, 2.25, 7, 0, 0, 0, 0, 0
