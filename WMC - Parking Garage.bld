	#External walls
	WallTex()
	Set 6 = ExtFacade_Stairs #Override
	Set 6L = ParkingWall3
	if[%floor% = 9] Set 6L = ExtFacade_Stairs

	Set ShowTop = false
	if[%floor% = 9] Set ShowTop = true
