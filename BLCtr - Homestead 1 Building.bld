#Set textures
<Function FloorTex>
	Set 5 = LobbyFloor
	if[%floor% > 0 & %floor% < 10] Set 5 = OfficeFloor
	if[%floor% = 10] Set 5 = EXOfficeFloor
	if[%floor% = 11] Set 5 = StorageFloor
	Set 5B = RoofFloor #Roof
	
	#Carpetways on lobby level
	Set 5C = %5%
	if[%floor% = 0] Set 5C = LobbyCarpet_Beige
	
	#Flooring on standard office levels
	Set 5F = %5%
	if[%floor% > 0 & %floor% < 10] Set 5F = OfficeLobbyFloor
	if[%floor% = 10] Set 5F = EXOfficeLobbyFloor
<EndFunction>

<Function WallTex>
	Set 6 = LobbyWall
	if[%floor% > 0 & %floor% < 10] Set 6 = OfficeWall
	if[%floor% = 10] Set 6 = EXOfficeWall
	if[%floor% = 11] Set 6 = StorageWall
	if[%floor% = 12] Set 6 = ExtFacade

		#Service/freight elevator lobby
		Set 6S = %6%
		if[%floor% ! 10 & %floor% ! 11] Set 6S = ServiceLobbyWall
		if[%floor% = 10] Set 6S = ServiceLobbyWall2
<EndFunction>

<Function CeilingTex>
	Set 7 = LobbyCeiling
	if[%floor% > 0 & %floor% < 10] Set 7 = OfficeCeiling
	if[%floor% = 10] Set 7 = EXOfficeCeiling
	if[%floor% = 11] Set 7 = StorageCeiling
<EndFunction>

