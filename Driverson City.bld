    AddStairsWall 1, FrontWall, StairsWall, 0.33, -8.5, -3.5, 8.5, -3.5, %fullheight%, %fullheight%, -%interfloorheight%, -%interfloorheight%, 0, 0
    WallOrientation = left
    DrawWalls = true, false, false, false, false, false
