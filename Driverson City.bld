    AddStairsWall 1, RightWall, StairsWall, 0.33, 8.5, -3.5, 8.5, 3.5, %fullheight%, %fullheight%, -%interfloorheight%, -%interfloorheight%, 0, 0

    #External stairwell walls
    if[%floor% < 10] CreateShaftWall(Wall1, Wall1, Wall1, Wall1, Wall1, Wall1, 0.33, 8, 16.17, 8.5 + 0.33, 3.5 + 0.33, 1, %height%, 0, true, true, true, false, false, false, false)
