#Room Signs
<Function AddSign>
	Set Name = %param1%
	Set BackTexture = %param2%
	Set Texture = %param3%
	Set MainX = %param4%
	Set MainZ = %param5%
	Set Direction = %param6%
	
	if[%Direction% = Front | %Direction% = front] Set Direction1 = right
	if[%Direction% = Left | %Direction% = left] Set Direction1 = right
	if[%Direction% = Right | %Direction% = right] Set Direction1 = left
	if[%Direction% = Back | %Direction% = back] Set Direction1 = left

	if[%Direction% = Front | %Direction% = front] Set Direction2 = true
	if[%Direction% = Left | %Direction% = left] Set Direction2 = false
	if[%Direction% = Right | %Direction% = right] Set Direction2 = false
	if[%Direction% = Back | %Direction% = back] Set Direction2 = true

	WallOrientation = %Direction1%
	if[%Direction1% = right] DrawWalls = true, false, true, true, true, true
	if[%Direction1% = left] DrawWalls = false, true, true, true, true, true
	if[%Direction2% = true] AddWall %Name%_Back, %BackTexture%, 0.1, %MainX% - 0.5, %MainZ%, %MainX% + 0.5, %MainZ%, 0.5, 0.5, 5.5, 5.5, 0, 0, false
	if[%Direction2% = false] AddWall %Name%_Back, %BackTexture%, 0.1, %MainX%, %MainZ% - 0.5, %MainX%, %MainZ% + 0.5, 0.5, 0.5, 5.5, 5.5, 0, 0, false

	if[%Direction1% = right] DrawWalls = true, false, false, false, false, false
	if[%Direction1% = left] DrawWalls = false, true, false, false, false, false
	if[%Direction1% = right & %Direction2% = true] AddWall %Name%, %Texture%, 0, %MainX% - 0.5, %MainZ% - 0.101, %MainX% + 0.5, %MainZ% - 0.101, 0.5, 0.5, 5.5, 5.5, 0, 0, false
	if[%Direction1% = right & %Direction2% = false] AddWall %Name%, %Texture%, 0, %MainX% - 0.101, %MainZ% - 0.5, %MainX% - 0.101, %MainZ% + 0.5, 0.5, 0.5, 5.5, 5.5, 0, 0, false
	if[%Direction1% = left & %Direction2% = true] AddWall %Name%, %Texture%, 0, %MainX% - 0.5, %MainZ% + 0.101, %MainX% + 0.5, %MainZ% + 0.101, 0.5, 0.5, 5.5, 5.5, 0, 0, false
	if[%Direction1% = left & %Direction2% = false] AddWall %Name%, %Texture%, 0, %MainX% + 0.101, %MainZ% - 0.5, %MainX% + 0.101, %MainZ% + 0.5, 0.5, 0.5, 5.5, 5.5, 0, 0, false
<EndFunction>

