<Function WallTex>
	if[%floor% = -1] Set 6 = BasementWall #Wall Texture
	if[%floor% > -1 & %floor% < 7] Set 6 = LobbyWall #Wall Texture
	if[%floor% > 6 & %floor% < 8] Set 6 = OfficeWall #Wall Texture
	if[%floor% > 8 & %floor% < 27] Set 6 = WardWall #Wall Texture
	if[%floor% > 26] Set 6 = ServiceWall2 #Wall Texture

	#Override wall texture for support columns
	Set 6A = %6%
	if[%floor% = -1] Set 6A = ColumnTex
	if[%floor% > 26] Set 6A = ColumnTex2

		#Escalator railings
		Set 6E = %6%
		if[%floor% = 1 | %floor% = 2] Set 6E = LobbyEscRail
	
		#Set textures for office walls
		Set 6Int = %6%
		if[%floor% > 6 & %floor% < 9] Set 6Int = OfficeIntWall
		if[%floor% > 8 & %floor% < 27] Set 6Int = WardIntWall
		if[%floor% > 26] Set 6Int = %6A%
		
		#--------------------#
		
		#Override for Heart Center (2nd floor) and ENT Center (3rd floor)
		Set HeartTex = HeartWall
		if[%floor% = 2] Set HeartTex = ENTWall #Keeping the variable name for compatibility reasons
		if[%floor% ! 1 & %floor% ! 2] Set HeartTex = %6Int%

		#Override for Eye Center, 3rd floor
		Set EyeTex = EyeWall
		if[%floor% ! 2] Set EyeTex = %6Int%

		#Override for GI Center, 5th floor
		Set GITex = GIWall
		if[%floor% ! 4] Set GITex = %6Int%

		#--------------------#

		#Set textures for support columns inside offices/patient rooms
		Set 6B = %6A%
		if[%floor% > 6 & %floor% < 9] Set 6B = OfficeIntWall
		if[%floor% > 8 & %floor% < 27] Set 6B = WardIntWall

		#Set textures for support columns that are INSIDE offices but OUTSIDE of patient rooms
		Set 6C = %6A%
		if[%floor% > 6 & %floor% < 9] Set 6C = OfficeIntWall

		#Set textures for support columns inside Eye Center
		Set 6Eye = %EyeTex%
		if[%floor% ! 2] Set 6Eye = %6B%

		#Set textures for support columns inside GI Center
		Set 6GI = %6B%
		if[%floor% = 2] Set 6GI = %6Eye%
		if[%floor% = 4] Set 6GI = %GITex%

		Set 6GI2 = %6B%
		if[%floor% = 4] Set 6GI2 = %GITex%

		#Set textures for support columns inside Heart and ENT Centers
		Set 6Heart = %HeartTex%
		if[%floor% ! 1 & %floor% ! 2] Set 6Heart = %6B%
<EndFunction>

