	TextureOverride ElevExt, ElevExt, ElevExt, %Prefix%ElevPanelMain, ElevExt, ElevExt
	AddWall GapFiller, %Prefix%ElevPanelMain, 0.15 * 2, %IDP%, -3, %IDP%, -2.75, 7, 7, 0, 0, 0, 0
	#--------------------
	#Button Panel
	Set P1_PositionX = 4.44 - 0.001
	Set P1_PositionZ = 2.275

