		WallOrientation = right
		AddStdDoor Floor %floor%, RR1Door_%floorid%, , , false, %DTex%, %DSTex%, 0.2, left, right, 75, 75, -44.5, 20 + 2.5, 3, 7, 0, 0, 0, 0, 0
		AddStdDoor Floor %floor%, RR2Door_%floorid%, , , false, %DTex%, %DSTex%, 0.2, right, right, 75, 75, -44.5, 20 - 2.5, 3, 7, 0, 0, 0, 0, 0
		AddFillerWalls ConnectionWall, 0.5, -44.5, 20 + 2.5, 3, 7, 0, false, 0, 0, false
		AddFillerWalls ConnectionWall, 0.5, -44.5, 20 - 2.5, 3, 7, 0, false, 0, 0, false
	
