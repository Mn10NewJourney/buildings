	if[%floor% ! 0 & %floor% ! 6] AddBoxSign(Stair%25%_Hallway, ExitSignRight, ExitSignLeft, StainlessSteel, 0.1, true, true, true, 0, %22% - %24% + 2, 1.8, 0.9, %height% - 0.9, false)
