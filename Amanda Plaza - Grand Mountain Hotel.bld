#Hotel Elevators
<Elevator 1>
	Name = Elevator 1
	Speed = 4
	Acceleration = 0.6
	Deceleration = 0.6
	ServicedFloors = 0 - 5
	AssignedShaft = 1
	CreateElevator true, 0.59, -5, 0
<EndElevator>

<Elevator 2>
	Name = Elevator 2
	Speed = 4
	Acceleration = 0.6
	Deceleration = 0.6
	ServicedFloors = 0 - 5
	AssignedShaft = 1
	CreateElevator true, 0.59, 5, 0
<EndElevator>

<Function BtnPanelE1-2>
	Set PanelNumber = %param1%

	AddButton %PanelNumber%, , Button6, ButtonLit6, 1, 2, 5, 1, 1
	AddButton %PanelNumber%, , Button5, ButtonLit5, 1, 1, 4, 1, 1
	AddButton %PanelNumber%, , Button4, ButtonLit4, 2, 2, 3, 1, 1
	AddButton %PanelNumber%, , Button3, ButtonLit3, 2, 1, 2, 1, 1
	AddButton %PanelNumber%, , Button2, ButtonLit2, 3, 2, 1, 1, 1
	AddButton %PanelNumber%, , Button1, ButtonLit1, 3, 1, 0, 1, 1

	AddButton %PanelNumber%, , ButtonOpen, ButtonOpen, 5, 1, open, 1, 1
	AddButton %PanelNumber%, , ButtonClose, ButtonClose, 5, 2, close, 1, 1
	if[%PanelNumber% = 1] AddControl %PanelNumber%, , 6, 1, 1, 1, 0, 0, 1, run, stop, ButtonStop, ButtonStop
	if[%PanelNumber% = 1] AddButton %PanelNumber%, , ButtonAlarm, ButtonAlarm, 6, 2, alarm, 1, 1
<EndFunction>

<Elevators 1 to 2>
	Set Prefix = Hotel

	#Set textures and dimensions
	M10_ElevCabTex(ElevExt, %Prefix%ElevFloor, %Prefix%ElevCeiling, %Prefix%ElevWall, %Prefix%ElevWall)
	M10_ElevDimensions(6.5, 7, 7.5, 0.2, 0.2, true, true, false, true)
	M10_ElevPanels(%Prefix%ElevPanelMain, %Prefix%ElevPanelTop, 1.5, 1.5, 0.5, 0.2, false, false, true, false)

	#Finish elevator assembly
	M10_ElevInterior()
	#--------------------
	#DETAILED INTERIORS
	#1. Lights
	Set LightWidth = 2.4
	Set LightLength = 3.6
	Set TrimWidth = 0.2

	CreateWallBox2 elevator, CL_Main, %Prefix%ElevCeilingLight, 0, 0, %LightWidth%, %LightLength%, 0.05, 7.5 - 0.05, 0, 0, false, true, true, true

	#2. Panels on walls
	Set WPTex = %Prefix%ElevWallPanel
	Set WPanelHeight = 3

		#Front/back panels
		CreateWallBox2 elevator, Panel_F1, %WPTex%, 0, -3.3 + 0.01, 6.1, 0.02, %WPanelHeight%, 0, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_B1, %WPTex%, 0, 3.3 - 0.01, 6.1, 0.02, %WPanelHeight%, 0, 0, 0, false, true, true, true

		#Left panels
		CreateWallBox2 elevator, Panel_L1, %WPTex%, -3.05 + 0.01, 0, 0.02, 6.6 - 0.04, %WPanelHeight%, 0, 0, 0, false, true, true, true
	#--------------------
	#Car Doors
	Set ElevDoorTex = %Prefix%ElevDoors
	Set IDP = 3.25

	WallOrientation = left
	AddDoorComponent 1, Left, %ElevDoorTex%, %ElevDoorTex%, 0.2, right, 0.8, 0.6, %IDP%, 0, %IDP%, 2, 7, 0, 0, 0, 0, 0
	AddDoorComponent 1, Right, %ElevDoorTex%2, %ElevDoorTex%2, 0.2, left, 0.8, 0.6, %IDP%, -2, %IDP%, 0, 7, 0, 0, 0, 0, 0
	FinishDoors 1, false, false

		#Door track
		FloorOrientation = top
		DrawWalls = true, true, true, true, true, true
		TextureOverride ElevExt, ElevDoorTrackX, ElevExt, ElevExt, ElevExt, ElevExt
		AddFloor DoorTrack, ElevDoorTrackX, 0.05, %IDP%, -2 - (2 * 1), %IDP% + 0.2, 2 + (2 * 1), 0, 0, 0, 0
	#--------------------
	#Right Panels
	Set P1_PositionX = 3.05 - 0.001
	Set P1_PositionZ = -2.65

		#Floor buttons
		CreateWallBox2 elevator, RightPB, ElevPanel, %P1_PositionX% + 0.001 - 0.025, %P1_PositionZ%, 0.05, 0.8, 4.75, 2.25, 0, 0, false, true, true, true
		CreatePanel , 6, 2, left, %P1_PositionX% - 0.05, %P1_PositionZ%, 0.26, 0.13, 0.15, 0.2, 3.75, 0, 0 #All buttons
		BtnPanelE1-2(1)
	#--------------------
	#Left Panel
	Set P2_PositionX = 3.05 - 0.001
	Set P2_PositionZ = 2.65

		#Floor buttons
		CreateWallBox2 elevator, LeftPB, ElevPanel, %P2_PositionX% + 0.001 - 0.025, %P2_PositionZ%, 0.05, 0.8, 4.75, 2.25, 0, 0, false, true, true, true
		CreatePanel , 6, 2, left, %P2_PositionX% - 0.05, %P2_PositionZ%, 0.26, 0.13, 0.15, 0.2, 3.75, 0, 0 #All buttons
		BtnPanelE1-2(2)

	#--------------------
	#Floor indicators
	AddFloorIndicator Ind_Button, left, %P1_PositionX% - 0.05, %P1_PositionZ%, 0.5, 0.286, 5.75 + 0.25 - 0.143
	AddFloorIndicator Ind_Button, left, %P2_PositionX% - 0.05, %P2_PositionZ%, 0.5, 0.286, 5.75 + 0.25 - 0.143

	#Elevator number sign
	DrawWalls = true, false, false, false, false, false
	AddWall ElevNo%elevator%_Inside, IN_ElevNo%elevator%, 0, %P1_PositionX% - 0.05, %P1_PositionZ% - 0.25, %P1_PositionX% - 0.05, %P1_PositionZ% + 0.25, 0.5, 0.5, 6.5, 6.5, 0, 0
	#--------------------
	#Shaft components
	DrawWalls = true, true, true, true, true, true
		#Ropes
		RopePosition = 0, 7.5 + 0.2, 0
		RopeTexture = Ropes
	
		#Counterweight
		Set Position = -3.25 - 0.75

		CreateCounterweight CWeight_Frame, CWeight, %Position%, 0, 0.5, 6, 2, 0.5
	
		#Rails
		AddRails ElevRail, ElevRailSide, 0, 0, true, 3.5 + 0.06, 0.1
		AddRails ElevRail, ElevRailSide, %Position%, 0, true, 1 + 0.06, 0.1
<EndElevators>

##--------------------------------------------------------------------------------##

#HALL STATIONS
#Set textures
<Function ElevHallTex>
	#Shaft doors
	TexPrefix() #Set prefixes for shaft doors
	Set DoorTexture = %5%ShaftDoors

	#Use automatic filler walls or not
	Set AutoFiller = false

	#Door trims
		#Sides
		Set DoorTrimTexture = %DoorTexture%_Trim

		#Top
		Set DoorTrimTexture_Top = %DoorTrimTexture%

	#Deco dimensions
		Set SideDecoH = 7.2
		Set TopDecoH = 0.2
		if[%floor% = 0] Set SideDecoH = %height%
		if[%floor% = 0] Set TopDecoH = %height% - 7

	#Call button panels
	Set BPanelTexture = ElevExtPanels
	Set ButtonPrefix = Button

	#Lantern panels
	Set LPanelTexture = %BPanelTexture%
	Set LanternPrefix = Lantern

		#Lateral offset for lantern panels
		Set ExtraLateralOffset = 0
		if[%floor% = 0] Set ExtraLateralOffset = 0.05
<EndFunction>

<Function E1-2FillerWalls>
	Set ElevNo = %param1%
	Set CenterZ = %param2%
	Set Suffix = Wall
	if[%floor% > 0] Set Suffix = LobbyWall

	#1. Outer trims
	#Main filler walls
	WallOrientation = right
	AddFillerWalls %5%%Suffix%, 0.66 - 0.15, %Shaft1CX% + 5, %CenterZ%, 4, 7, 0, false, 0, 0, false

	#2. Inner trims
	WallOrientation = right
	AddFillerWalls %DoorTrimTexture%, 0.15, %Shaft1CX% + 5 - 0.51, %CenterZ%, 4, 7, 0, false, 0, 0, false
<EndFunction>

<Floors 0 to 5>
	#Shaft Doors
	ElevHallTex() #Set shaft door and panel textures
	Set StartNo = 1 #Number of the first elevator

	Set IDP = 3.25 + 0.3
	WallOrientation = left
	AddShaftDoorComponent %StartNo%, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, left, 0.8, 0.6, %IDP%, -2, %IDP%, 0, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo%, 1, Right, %DoorTexture%2, %DoorTexture%2, 0.2, right, 0.8, 0.6, %IDP%, 0, %IDP%, 2, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo%, 1, %AutoFiller%, false

	WallOrientation = left
	AddShaftDoorComponent %StartNo% + 1, 1, Left, %DoorTexture%, %DoorTexture%, 0.2, left, 0.8, 0.6, %IDP%, -2, %IDP%, 0, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo% + 1, 1, Right, %DoorTexture%2, %DoorTexture%2, 0.2, right, 0.8, 0.6, %IDP%, 0, %IDP%, 2, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo% + 1, 1, %AutoFiller%, false

	#Door tracks
	AddShaftTrack(1, 1, 4.14, 4.34, -5 - 4, -5 + 4, ElevDoorTrackX, ElevExt, 0.05, 1, 7)
	AddShaftTrack(2, 1, 4.14, 4.34, 5 - 4, 5 + 4, ElevDoorTrackX, ElevExt, 0.05, 1, 7)

	#****************************************#

	#Door trims/fillers
	WallTex() #Set filler textures to match with shaft walls
	Set Shaft1CX = -10 #1st shaft for elevator bank
	Set Shaft1CZ = 50

		#1st floor only
		if[%floor% = 0] CreateWallBox2 floor, E1Deco, %DoorTexture%_Deco, %Shaft1CX% + 5.025, %Shaft1CZ% - 5, 0.05, 3.75, 0.9, 7.5 + 0.25 - 0.45, 0, 0, false, true, true, true 
		if[%floor% = 0] CreateWallBox2 floor, E2Deco, %DoorTexture%_Deco, %Shaft1CX% + 5.025, %Shaft1CZ% + 5, 0.05, 3.75, 0.9, 7.5 + 0.25 - 0.45, 0, 0, false, true, true, true 

		#if[%floor% = 0] AddDecoObject(Elev1_Deco, %DoorTrimTexture%, %DoorTrimTexture_Top%, right, true, false, 0.05, 0.2, %Shaft1CX% + 5, %Shaft1CZ% - 5, 4, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66, floor)
		#if[%floor% = 0] AddDecoObject(Elev2_Deco, %DoorTrimTexture%, %DoorTrimTexture_Top%, right, true, false, 0.05, 0.2, %Shaft1CX% + 5, %Shaft1CZ% + 5, 4, 7, %SideDecoH%, %TopDecoH%, 0, true, %DoorTrimTexture%, 0.66, floor)

		#All floors
		E1-2FillerWalls(1, %Shaft1CZ% - 5)
		E1-2FillerWalls(2, %Shaft1CZ% + 5)

	#****************************************#

	#Call buttons
	CallButtonElevators = 1, 2
	CreateCallButtons %BPanelTexture%, %ButtonPrefix%Up, %ButtonPrefix%UpLit, %ButtonPrefix%Down, %ButtonPrefix%DownLit, %Shaft1CX% + 5.001, %Shaft1CZ%, 3.6, right, 0.4, 0.8, true, 0, 0

		#Signs
		Set Suffix = %floorid%

		DrawWalls = false, true, false, false, false, false
		if[%floor% = 0] AddWall E1-2FloorSign, BlankSign%Suffix%, 0, %Shaft1CX% + 5.001, %Shaft1CZ% - 0.75, %Shaft1CX% + 5.001, %Shaft1CZ% + 0.75, 1.5, 1.5, 5.25, 5.25, 0, 0, false
		if[%floor% > 0] AddWall E1-2FloorSign, FlrSign%Suffix%, 0, %Shaft1CX% + 5.001, %Shaft1CZ% - 0.5, %Shaft1CX% + 5.001, %Shaft1CZ% + 0.5, 1, 1, 5.5, 5.5, 0, 0, false
		AddWall E1-2HallSign, FireSign, 0, %Shaft1CX% + 5.001, %Shaft1CZ% - 0.25, %Shaft1CX% + 5.001, %Shaft1CZ% + 0.25, 0.75, 0.75, 2.6, 2.6, 0, 0, false

	#****************************************#

	#Indicator sets
	Set Offset = 7.5
	#if[%floor% = 0] Set Offset = 7.75

		#Set textures
		ACMEInd_LanternTex(%LanternPrefix%Up, %LanternPrefix%UpLit, %LanternPrefix%Down, %LanternPrefix%DownLit, %LPanelTexture%, Ind_Button)

		#Indicator creation
		if[%floor% = 0] ACMELobbyInds(right, 1, %Shaft1CX% + 5 + %ExtraLateralOffset%, %Shaft1CZ% - 5, %Offset%, 0, 5, true)
		if[%floor% = 0] ACMELobbyInds(right, 2, %Shaft1CX% + 5 + %ExtraLateralOffset%, %Shaft1CZ% + 5, %Offset%, 0, 5, true)
		if[%floor% ! 0] ACMELobbyLanterns(right, 1, %Shaft1CX% + 5 + %ExtraLateralOffset%, %Shaft1CZ% - 5, %Offset%, 0, 5, true)
		if[%floor% ! 0] ACMELobbyLanterns(right, 2, %Shaft1CX% + 5 + %ExtraLateralOffset%, %Shaft1CZ% + 5, %Offset%, 0, 5, true)
<EndFloors>

#Elevator settings
<Elevators 1 to 2>
	#Notifications
	UpChimeSound 1 = EG881Sounds/MTupchime2.wav #EG881
	DownChimeSound 1 = EG881Sounds/MTdownchime2.wav #EG881
	BeepSound = EG881Sounds/MTbeep2.wav #EG881
	NotifyEarly = 1
	QueueResets = true

	#Door settings
	DoorTimer 1 = 9000
	QuickClose 1 = 6000
	OpenSound 1 = EG881Sounds/elevopenquick2.wav #EG881
	CloseSound 1 = EG881Sounds/elevclosequick2.wav #EG881

	#Movement sounds
	MotorStartSound = EG881Sounds/motor_start3.wav #EG881
	MotorRunSound = EG881Sounds/motor_running3.wav #EG881
	MotorStopSound = EG881Sounds/motor_stop3.wav #EG881

	CarStartSound = EG881Sounds/slowelevstart.wav #EG881
	CarMoveSound = EG881Sounds/slowelevmove.wav #EG881
	CarStopSound = EG881Sounds/slowelevstop.wav #EG881
<EndElevators>