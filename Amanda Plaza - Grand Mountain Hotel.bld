##--------------------------------------------------------------------------------##

#Floor creation function
<Function LSFloorCreator>
	Set FlrName = %param1%
	Set FlrTex = %param2%
	Set X1 = %param3%
	Set Z1 = %param4%
	Set X2 = %param5%
	Set Z2 = %param6%
	Set Offset = %param7%
	Set RA = %param8% #Reverse axis
	Set TD = %param9% #Texture dir.
	
	#Cut ground for floors
	Cut Landscape, %X1%, %Offset% - 1, %Z1%, %X2%, %Offset% + 1, %Z2%, false, true

	#Floor creation
	DrawWalls = false, true, false, false, false, false
	AddFloor landscape, %FlrName%, %FlrTex%, 0, %X1%, %Z1%, %X2%, %Z2%, %Offset%, %Offset%, %RA%, %TD%, 0, 0	
<EndFunction>

##--------------------------------------------------------------------------------##

#Common values
Set 1 = 40 #Building Dimension X
Set 2 = 60 #Building Dimension Z

#Landscape
	#1. Perimeter floors
	DrawWalls = false, true, false, false, false, false

	Set FO = Floor(0).Base #Floor Offset
	LSFloorCreator(NorthFloor, Grass, -%1% - 10, %2%, %1% + 20, %2% + 5, %FO%, false, false)
	LSFloorCreator(SouthFloor1, Grass_Dark, -%1% - 10, -%2% - 10, -5, -%2%, %FO%, false, false)
	LSFloorCreator(SouthFloor2, Sidewalk, -5, -%2% - 10, %1% + 20, -%2%, %FO%, false, true)
	LSFloorCreator(EastFloor, Grass_Dark, %1%, -%2%, %1% + 20, %2%, %FO%, false, false)
	LSFloorCreator(EastFloor_Main, Sidewalk, %1%, -10, %1% + 20, 10, %FO%, false, false)
	LSFloorCreator(WestFloor, Grass_Dark, -%1% - 10, -%2%, -%1%, %2%, %FO%, false, false)
	LSFloorCreator(WestFloor_Main, Sidewalk, -%1% - 10, -10, -%1%, 10, %FO%, false, false)

