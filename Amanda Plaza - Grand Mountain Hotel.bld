##--------------------------------------------------------------------------------##

#Set texture prefixes/suffixes
<Function TexPrefix>
	#Main prefixes
	Set 5 = Lobby
	if[%floor% > 0] Set 5 = Hotel
	
	#Special for hotel walls
		#Elevator lobby
		Set HotelSuffix = Wall
		if[%floor% > 0 & %floor% < 6] Set HotelSuffix = LobbyWall

		#Hallways
		Set HHS = Wall #HHS = "Hotel Hallway Suffix"
		if[%floor% > 0 & %floor% < 6] Set HHS = Wall2
		
		#Corner walls
		Set CS = Wall2 #CS = "Corner Suffix"
		if[%floor% > 0] Set CS = Wall

	#Shaft doors for service elevators
	Set 5S = ServiceA
	if[%floor% > 0] Set 5S = ServiceB
<EndFunction>

