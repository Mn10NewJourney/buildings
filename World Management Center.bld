	if[%floor% = 110] Set 5 = UpperLevelsFloor #Floor Texture
	if[%floor% > 110] Set 5 = ServiceFloor2 #Floor Texture
<EndFunction>

<Function WallTex> #Set wall textures
	if[%floor% = 0 | %floor% = 1] Set 6 = LobbyWall #Wall Texture
	if[%floor% > 1 & %floor% < 39] Set 6 = OfficeWall #Wall Texture
	if[%floor% = 39 | %floor% = 40 | %floor% = 41 | %floor% = 42] Set 6 = ServiceWall #Wall Texture
	if[%floor% = 43] Set 6 = LobbyWall2 #Wall Texture
