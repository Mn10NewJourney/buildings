	DrawWalls = true, true, false, false, false, false
	AddWall %RName%_Mid, %6%, 1, %CtrX%, %CtrZ% - %DimZ%, %CtrX%, %CtrZ% + %DimZ%, %RHeight%, %RHeight%, 0, 0, 0, 0, false

