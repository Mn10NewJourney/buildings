<Function OfficeLvRestrooms>
	#This is for restrooms sandwiched between elevator shafts
	Set CtrX = %param1%
	Set RName = %param2%
	Set NorthSign = %param3%
	Set SouthSign = %param4%
	Set DTex = %param5%
	Set DSTex = %param6%
	
