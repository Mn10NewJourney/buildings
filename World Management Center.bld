	if[%floor% > 85 & %floor% < 108] Set 6 = OfficeWall3 #Wall Texture
	if[%floor% = 108 | %floor% = 109] Set 6 = ServiceWall #Wall Texture
	if[%floor% = 110] Set 6 = UpperLevelsWall #Wall Texture
	if[%floor% > 110] Set 6 = ServiceWall #Wall Texture
