		Set RestroomHeight = 8.5
		if[%floor% = 0] Set RestroomHeight = 9.5

		FloorOrientation = bottom
		DrawWalls = true, false, false, false, false, false
		AddFloor %RName%_Ceiling, DropCeiling, 0.5, %CtrX% - 5, -15, %CtrX% + 5, 15, %RestroomHeight%, %RestroomHeight%, 0, 0, false

	#Restroom doors
	WallOrientation = right
	AddStdDoor Floor %floor%, %RName%_Front, , , false, %DTex%, %DSTex%, 0.2, front, back, 75, 75, %CtrX%, -15 + 0.5, 3, 7, 0, 0, 0, 0, 0
	AddFillerWalls ConnectionWall, 0.5, %CtrX%, -15 + 0.5, 3, 7, 0, true, 0, 0, false
	WallOrientation = left
	AddStdDoor Floor %floor%, %RName%_Back, , , false, %DTex%, %DSTex%, 0.2, front, front, 75, 75, %CtrX%, 15 - 0.5, 3, 7, 0, 0, 0, 0, 0
