	if[%floor% = 1] Set SOffset = -%interfloorheight%
	if[%floor% = 40 | %floor% = 41 | %floor% = 42 | %floor% = 83 | %floor% = 84] Set SHeight = %fullheight%
	if[%floor% = 40 | %floor% = 41 | %floor% = 42 | %floor% = 83 | %floor% = 84] Set SOffset = -%interfloorheight%

