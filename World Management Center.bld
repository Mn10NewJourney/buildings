	WallOrientation = center
	DrawWalls = true, true, false, false, false, false
	AddWall %RName%_Mid, %6%, 1, %CtrX% - 5, 0, %CtrX% + 5, 0, %RHeight%, %RHeight%, 0, 0, 0, 0, false

	WallOrientation = left
	AddWall %RName%_Front, %6%, 0.5, %CtrX% - 5, -15, %CtrX% + 5, -15, %RHeight%, %RHeight%, 0, 0, 0, 0, false
	WallOrientation = right
	AddWall %RName%_Back, %6%, 0.5, %CtrX% - 5, 15, %CtrX% + 5, 15, %RHeight%, %RHeight%, 0, 0, 0, 0, false

