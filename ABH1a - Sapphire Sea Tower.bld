#Stairwells
<Function StairwellSets>
	#Set Values
	if[%floor% < 4 | %floor% > 34] Set NumSteps = 12 #Total: 24 steps
	if[%floor% > 3 & %floor% < 35] Set NumSteps = 10 #Total: 20 steps

	#Extra sets at transition points
	Set NumStepsAlt = %NumSteps%
	if[%floor% = 4] Set NumStepsAlt = 12
	Set NumStepsAlt2 = %NumSteps%
	if[%floor% = 35] Set NumStepsAlt2 = 10
<EndFunction>

#South Stairwell
CreateStairwell 1, -30, -148, 0, 41
CutStairwell 1, -5.5, -12.5, 5.5, 12.5, 0, 5
ShowFullStairs 1 = always

<Floors 0 to 41>
	StairwellSets() #Determine stairs' size

	Set 21 = -30 #Center X
	Set 22 = -148 #Center Z
	Set 23 = 5.5 #Cut X
	Set 24 = 12.5 #Cut Z
	Set 25 = 1 #Stairwell Number
	Set 26 = 0.5 #Riser Size
	
	#Walls
	WallOrientation = right
	DrawWalls = false, true, false, false, false, false
	AddStairsWall %25%, Stair%25%Front, StairsWall, 0.5, -%23%, -%24%, %23%, -%24%, %fullheight%, %fullheight%, -%interfloorheight%, -%interfloorheight%, 0, 0
	AddStairsWall %25%, Stair%25%Left, StairsWall, 0.5, -%23%, -%24%, -%23%, %24%, %fullheight%, %fullheight%, -%interfloorheight%, -%interfloorheight%, 0, 0
	WallOrientation = left
	DrawWalls = true, false, false, false, false, false
	AddStairsWall %25%, Stair%25%Right, StairsWall, 0.5, %23%, -%24%, %23%, %24%, %fullheight%, %fullheight%, -%interfloorheight%, -%interfloorheight%, 0, 0
	AddStairsWall %25%, Stair%25%Back, StairsWall, 0.5, -%23%, %24%, %23%, %24%, %fullheight%, %fullheight%, -%interfloorheight%, -%interfloorheight%, 0, 0

	WallOrientation = center
	DrawWalls = true, true, true, true, true, true
	AddStairsWall %25%, Stair%25%Center, StairsWall2, 1, 0, %24% - 5 - %NumSteps%, 0, %24% - 5, %fullheight%, %fullheight%, -%interfloorheight%, -%interfloorheight%, 0, 0
	if[%floor% = 4] AddStairsWall %25%, Stair%25%Center2, StairsWall2, 1, 0, %24% - 5 - %NumSteps%, 0, %24% - 5 - %NumStepsAlt%, %interfloorheight% + 3, %interfloorheight% + 3, -%interfloorheight%, -%interfloorheight%, 0, 0

	#Railing
	WallOrientation = left
	DrawWalls = true, true, true, false, true, true
	if[%floor% = 41] AddStairsWall %25%, Stair%25%Rail, StairsWall2, 0.5, -%23%, %24% - 5 - %NumSteps%, -%23% + 5, %24% - 5 - %NumSteps%, 0.5 + 3.5, 0.5 + 3.5, -0.5, -0.5, 0, 0

	#Ceiling
	FloorOrientation = top
	DrawWalls = true, false, false, false, false, false
	if[%floor% = 41] AddStairsFloor %25%, Stairs%25%Ceiling, StairsCeiling, 0.5, -%23%, -%24%, %23%, %24%, %height%, %height%, false, false, 0, 0

	#Stairs
	OP_Draw(true, true, true, true, true, true)
	if[%floor% < 41] OP_OrthoStair(%25%, ST%25%_LowerStairs, Stairs, -%23%, -%23% + 5, front, %24% - 5 - %NumSteps%, %NumSteps%, 1, %26%, 0, 0.5)
	if[%floor% < 41] OP_OrthoStair(%25%, ST%25%_UpperStairs, Stairs, %23% - 5, %23%, back, %24% - 5, %NumSteps%, 1, %26%, %26% * %NumSteps%, 0.5)

	#Landings
	FloorOrientation = top
	DrawWalls = true, true, false, false, false, false
	if[%floor% = 0] AddStairsFloor %25%, Stairs%25%LandingLower, StairsFloor, 0.5, -%23%, -%24%, %23%, %24%, 0, 0, false, false, 0, 0
	if[%floor% > 0] AddStairsFloor %25%, Stairs%25%LandingLower, StairsFloor, 0.5, -%23%, -%24%, %23%, %24% - 5 - %NumStepsAlt%, 0, 0, false, false, 0, 0
	if[%floor% < 41] AddStairsFloor %25%, Stairs%25%LandingUpper, StairsFloor, 0.5, -%23%, %24% - 5, %23%, %24%, %26% * %NumSteps%, %26% * %NumSteps%, false, false, 0, 0

	if[%floor% = 4] AddStairsFloor %25%, Stairs%25%Transition1, StairsFloor, 0.5, -%23%, %24% - 5 - %NumSteps%, -%23% + 5, %24% - 5 - %NumStepsAlt%, 0, 0, false, false, 0, 0
	if[%floor% = 35] AddStairsFloor %25%, Stairs%25%Transition2, StairsFloor, 0.5, %23% - 5, %24% - 5 - %NumSteps%, %23%, %24% - 5 - %NumStepsAlt2%, 0, 0, false, false, 0, 0

	#Floor Signs
	Set Suffix = %floorid%
	Set SignCenterPoint = 0

	DrawWalls = false, true, false, false, false, false
	AddStairsWall %25%, ST%25%FloorSign, BlankSign%Suffix%, 0, %SignCenterPoint% - 1, -%24% + 0.001, %SignCenterPoint% + 1, -%24% + 0.001, 2, 2, 4, 4, 0, 0

	#External walls
	WallTex()
	CreateShaftWall(%6%, %6%, %6%, %6%, %6%, %6%, 0.5, %21%, %22%, %23% + 0.5, %24% + 0.5, %25%, %height%, 0, false, true, true, true, false, false, false)

	#Doors
	WallOrientation = left
	TextureOverride ServiceDoor2, ServiceDoor2B, ServiceDoor2Side, ServiceDoor2Side, ServiceDoor2Side, ServiceDoor2Side
	AddStairsDoor %25%, , , false, ServiceDoor2, 0.2, 1, 75, %23%, -%24% + 2, 3, 7, 0, 0, 0
	SetLock 1, 0
	TextureOverride ServiceDoor2, ServiceDoor2B, ServiceDoor2Side, ServiceDoor2Side, ServiceDoor2Side, ServiceDoor2Side
	if[%floor% = 0] AddStairsDoor %25%, , , false, ServiceDoor2, 0.2, 5, 75, -%23% + 2, -%24% - 0.5, 3, 7, 0, 0, 0
	SetLock 0, 0
	
		#Filler walls
		WallOrientation = left
		#AddFillerWalls ConnectionWall, 0.5, %21% + %23%, %22% - %24% + 2, 3, 7 - 0.001, 0, false, 0, 0, false
		if[%floor% = 0] AddFillerWalls ConnectionWall, 0.5, %21% - %23% + 2, %22% - %24% - 0.5, 3, 7 - 0.001, %interfloorheight%, true, 0, 0, true

	#Exit Signs
	#if[%floor% < 4 | %floor% > 34] AddBoxSign(Stair%25%_SignA1, StainlessSteel, ExitSign, StainlessSteel, 0.1, false, true, true, %21% + %23% + 0.55, %22% - %24% + 2, 1.8, 0.9, %interfloorheight% + 7.2, true)

	#Set SignTop = %height%
	Set SignOffset = 8
	if[%floor% > 3 & %floor% < 35] Set SignOffset = %height% - 0.9
	if[%floor% ! 0] AddBoxSign(Stair%25%_SignA2, StainlessSteel, ExitSignRight, StainlessSteel, 0.1, true, true, true, -24 + 0.9, %22% - %24% + 2, 1.8, 0.9, %SignOffset%, false)
	if[%floor% = 0] AddBoxSign(Stair%25%_SignB, StainlessSteel, ExitSign, StainlessSteel, 0.1, true, true, true, %21% - %23% + 2, %22% - %24% + 0.05, 1.8, 0.9, %interfloorheight% + 7.2, true)
<EndFloors>

