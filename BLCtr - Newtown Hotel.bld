##--------------------------------------------------------------------------------##

#Set textures
<Function FloorTex>
	Set 5 = LobbyFloor
	if[%floor% > 0] Set 5 = GuestroomFloor
	Set 5B = RoofFloor #Roof

		#Elevator machinery enclosures (Roof level)
		Set 5Q = %5%
		if[%floor% = 8] Set 5Q = ElevMRIntFloor
<EndFunction>

<Function WallTex>
	Set 6 = LobbyWall
	if[%floor% > 0 & %floor% < 8] Set 6 = GuestroomWall
	if[%floor% = 8] Set 6 = ExtFacade

		#Service elevator lobby
		Set 6S = %6%
		if[%floor% ! 0 & %floor% ! 8] Set 6S = ServiceLobbyWall2
		if[%floor% = 0] Set 6S = ServiceLobbyWall

		#Elevator machinery enclosures (Roof level)
		Set 6Q = %6%
		if[%floor% = 8] Set 6Q = ElevMRIntWall
<EndFunction>

<Function CeilingTex>
	Set 7 = LobbyCeiling
	if[%floor% > 0] Set 7 = GuestroomCeiling

		#Elevator machinery enclosures (Roof level)
		Set 7Q = %7%
		if[%floor% = 8] Set 7Q = ElevMRIntCeiling
<EndFunction>

