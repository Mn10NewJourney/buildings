	if[%floor% > 0 & %floor% < 5] Set 5L = RoomLobbyFloor
	if[%floor% = 5] Set 5L = UpperLobbyFloor
<EndFunction>

<Function WallTex>
	if[%floor% = 0] Set 6 = LobbyWall #Wall Texture
	if[%floor% > 0 & %floor% < 5] Set 6 = RoomWall
	if[%floor% = 5] Set 6 = UpperWall
	if[%floor% = 6] Set 6 = ServiceWall

