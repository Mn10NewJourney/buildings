<Elevators 1 to 3>
	Set Prefix = Original

	#Set textures and dimensions	
	M10_ElevCabTex(ElevExt2, %Prefix%ElevFloor, %Prefix%ElevCeiling, %Prefix%ElevWall, %Prefix%ElevWall)
	M10_ElevDimensions(7.68, 6.5, 7.5, 1, 0.2, true, false, false, true)
	M10_ElevPanels(%Prefix%ElevPanelMain, %Prefix%ElevPanelTop, 1.5, 1.5, 0.5, 0.2, false, false, true, false)

	#Finish elevator assembly
	M10_ElevInterior()
	#--------------------
	#Glass walls
	Set WPTex = %Prefix%ElevWallGlassLink
	Set GlassTex = %Prefix%ElevWallGlass
	Set WPanelHeight = 7.5

	WallOrientation = left
	DrawWalls = true, true, false, true, false, false
	AddWall GlassWallLink1, %WPTex%, 0.2, -3.84, -3.25, -3.84, -3.25 + 0.5, %WPanelHeight%, %WPanelHeight%, 0, 0, 0, 0
	DrawWalls = true, true, true, false, false, false
	AddWall GlassWallLink2, %WPTex%, 0.2, -3.84, 3.25 - 0.5, -3.84, 3.25, %WPanelHeight%, %WPanelHeight%, 0, 0, 0, 0
	#DrawWalls = true, true, false, false, false, true
	#AddWall GlassWallLink3, %WPTex%, 0.2, -3.84, -3.25, -3.84, 3.25, 0.25, 0.25, %WPanelHeight%, %WPanelHeight%, 0, 0

	DrawWalls = true, true, false, false, false, false
	AddWall GlassWall, %GlassTex%, 0, -3.84, -3.25 + 0.5, -3.84, 3.25 - 0.5, %WPanelHeight%, %WPanelHeight%, 0, 0, 0, 0
	#--------------------
	#DETAILED INTERIORS
	#1. Ceiling lights
	Set LightWidth = 7
	Set LightLength = 1.25

	Cut elevator, 0 - (%LightWidth% / 2), 7.5 - 0.1, -2.325 - (%LightLength% / 2), 0 + (%LightWidth% / 2), 7.5 + 0.1, -2.325 + (%LightLength% / 2), false, true
	Cut elevator, 0 - (%LightWidth% / 2), 7.5 - 0.1, 2.325 - (%LightLength% / 2), 0 + (%LightWidth% / 2), 7.5 + 0.1, 2.325 + (%LightLength% / 2), false, true

		#Light creation
		Set LightOffset1 = 7.75
		Set LightOffset2 = 7.5
		Set LEHight = %LightOffset1% - %LightOffset2% #LE = Light Edge

		CreateWallBox2 elevator, Light1_Edge, %Prefix%ElevCeilingLight2, 0, -2.325, %LightWidth%, %LightLength%, %LEHight%, %LightOffset2%, 0, 0, true, false, false, false 
		CreateWallBox2 elevator, Light1_Main, %Prefix%ElevCeilingLight, 0, -2.325, %LightWidth%, %LightLength%, 0.1, %LightOffset1% - 0.1, 0, 0, false, true, true, true 

		CreateWallBox2 elevator, Light2_Edge, %Prefix%ElevCeilingLight2, 0, 2.325, %LightWidth%, %LightLength%, %LEHight%, %LightOffset2%, 0, 0, true, false, false, false 
		CreateWallBox2 elevator, Light2_Main, %Prefix%ElevCeilingLight, 0, 2.325, %LightWidth%, %LightLength%, 0.1, %LightOffset1% - 0.1, 0, 0, false, true, true, true 

	#2. Panels on walls
	Set WPTex = %Prefix%ElevWallPanel

		#Front panels
		CreateWallBox2 elevator, Panel_F1, %WPTex%, 0 - (7.28 * 1/4), -3.05 + 0.01, (7.28 * 1/2) - 0.3, 0.02, 7.5 - 0.5, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_F2, %WPTex%, 0 + (7.28 * 1/4), -3.05 + 0.01, (7.28 * 1/2) - 0.3, 0.02, 7.5 - 0.5, 0.25, 0, 0, false, true, true, true

		#Back panels
		CreateWallBox2 elevator, Panel_B1, %WPTex%, 0 - (7.28 * 1/4), 3.05 - 0.01, (7.28 * 1/2) - 0.3, 0.02, 7.5 - 0.5, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_B2, %WPTex%, 0 + (7.28 * 1/4), 3.05 - 0.01, (7.28 * 1/2) - 0.3, 0.02, 7.5 - 0.5, 0.25, 0, 0, false, true, true, true
	#--------------------
	#Car Doors
	Set ElevDoorTex = %Prefix%ElevDoors
	Set IDP = 3.84

	WallOrientation = left
	AddDoorComponent 1, Left, %ElevDoorTex%, %ElevDoorTex%, 0.2, right, 0.8, 0.6, %IDP%, 0, %IDP%, 1.75, 7, 0, 0, 0, 0, 0
	AddDoorComponent 1, Right, %ElevDoorTex%2, %ElevDoorTex%2, 0.2, left, 0.8, 0.6, %IDP%, -1.75, %IDP%, 0, 7, 0, 0, 0, 0, 0
	FinishDoors 1, false, false

		#Door track
		FloorOrientation = top
		DrawWalls = true, true, true, true, true, true
		TextureOverride ElevExt, ElevDoorTrackX, ElevExt, ElevExt, ElevExt, ElevExt
		AddFloor DoorTrack, ElevDoorTrackX, 0.05, %IDP%, -1.75 - (1.75 * 1), %IDP% + 0.2, 1.75 + (1.75 * 1), 0, 0, 0, 0
	#--------------------
	#Right Panels
	Set P1_PositionX = 3.64 - 0.001
	Set P1_PositionZ = -2.4

		#Floor buttons
		CreateWallBox2 elevator, RightPB, ElevPanel, %P1_PositionX% + 0.001 - 0.025, %P1_PositionZ%, 0.05, 0.6, 3.5, 2.5, 0, 0, false, true, true, true
		CreatePanel Blank, 8, 2, left, %P1_PositionX% - 0.05, %P1_PositionZ%, 0.3, 0.3, -0.2, -0.400, 4, 0, 0
		BtnPanelE1-3(1)
		
		#Intercom panel
		CreatePanel BlackPanel, 3, 3, left, %P1_PositionX% - 0.05, %P1_PositionZ%, 0.1, 0.1, 0.2, 0.2, 5.25, 0, 0
		HitachiElev_Intercom(2)
	#--------------------
	#Left Panel
	Set P2_PositionX = 3.64 - 0.001
	Set P2_PositionZ = 2.4

		#Floor buttons
		CreateWallBox2 elevator, LeftPB, ElevPanel, %P2_PositionX% + 0.001 - 0.025, %P2_PositionZ%, 0.05, 0.6, 2.75, 2.5, 0, 0, false, true, true, true
		CreatePanel Blank, 8, 2, left, %P2_PositionX% - 0.05, %P2_PositionZ%, 0.3, 0.3, -0.2, -0.400, 4, 0, 0
		BtnPanelE1-3(3)
	#--------------------

	#Floor indicator
	AddFloorIndicator Ind_Button, left, 3.64 - 0.001, 0, 0.8, 0.4, 7.25 - 0.2

	#Elevator number sign
	DrawWalls = true, false, false, false, false, false
	AddWall ElevNo%elevator%_Inside, IN_ElevNo%elevator%, 0, %P1_PositionX% - 0.001, %P1_PositionZ% - 0.25, %P1_PositionX% - 0.001, %P1_PositionZ% + 0.25, 0.5, 0.5, 6.25, 6.25, 0, 0

	#--------------------
	#Shaft components
	DrawWalls = true, true, true, true, true, true
		#Ropes
		RopePosition = 0, 8.5, 0
		RopeTexture = Ropes
	
		#Counterweight
		Set Position = 3.25 + 0.65
		if[%elevator% = 3] Set Position = -3.25 - 0.65

		CreateCounterweight CWeight_Frame, CWeight, 0, %Position%, 2, 6, 0.5, 0.5

		#Rails
		AddRails ElevRail, ElevRailSide, 0, 0, true, 3.25 + 0.06, 0.1
		AddRails ElevRail, ElevRailSide, 0, %Position%, false, 1 + 0.06, 0.1
<EndElevators>

