	Name = West Elevator 1
	Type = Service
	Speed = 3.5
	Acceleration = 0.6
	Deceleration = 0.6
	ServicedFloors = 0 - 5
	AssignedShaft = 2
	MotorPosition = 0, Floor(6).Altitude + Floor(6).FullHeight - 0.5, 0
	CreateElevator false, -38.5, -134.16, 0
<EndElevator>

<Elevator 4>
	Name = West Elevator 2
	Type = Service
	Speed = 3.5
	Acceleration = 0.6
	Deceleration = 0.6
	ServicedFloors = 0 - 5
	AssignedShaft = 2
	MotorPosition = 0, Floor(6).Altitude + Floor(6).FullHeight - 0.5, 0
	CreateElevator false, -48.5, -134.16, 0
<EndElevator>

<Elevators 3 to 4>
	Set Prefix = West

	#Interiors
	ElevInterior(%Prefix%ElevFloor, %Prefix%ElevCeiling, %Prefix%ElevWall, %Prefix%ElevWall, 7.2, 5, back, 7.25)

	#Panels on walls
		#Front panels
		CreateWallBox2 elevator, Panel_F1, %Prefix%ElevWallPanel, 0 - (6.8 * 1/3), -2.3 + 0.01, (6.8 * 1/3) - 0.3, 0.02, 7.25 - 0.5, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_F2, %Prefix%ElevWallPanel, 0, -2.3 + 0.01, (6.8 * 1/3) - 0.3, 0.02, 7.25 - 0.5, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_F3, %Prefix%ElevWallPanel, 0 + (6.8 * 1/3), -2.3 + 0.01, (6.8 * 1/3) - 0.3, 0.02, 7.25 - 0.5, 0.25, 0, 0, false, true, true, true

		#Left panels
		CreateWallBox2 elevator, Panel_L1, %Prefix%ElevWallPanel, -3.4 + 0.01, 0 - (4.6 * 1/4), 0.02, (4.6 * 1/2) - 0.3, 7.25 - 0.5, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_L2, %Prefix%ElevWallPanel, -3.4 + 0.01, 0 + (4.6 * 1/4), 0.02, (4.6 * 1/2) - 0.3, 7.25 - 0.5, 0.25, 0, 0, false, true, true, true

		#Right panels
		CreateWallBox2 elevator, Panel_R1, %Prefix%ElevWallPanel, 3.4 - 0.01, 0 - (4.6 * 1/4), 0.02, (4.6 * 1/2) - 0.3, 7.25 - 0.5, 0.25, 0, 0, false, true, true, true
		CreateWallBox2 elevator, Panel_R2, %Prefix%ElevWallPanel, 3.4 - 0.01, 0 + (4.6 * 1/4), 0.02, (4.6 * 1/2) - 0.3, 7.25 - 0.5, 0.25, 0, 0, false, true, true, true

	#Panels
	WallOrientation = right
	DrawWalls = true, true, false, true, false, false
	TextureOverride %Prefix%ElevPanelMain, ElevExt, ElevExt, %Prefix%ElevPanelMain, ElevExt, ElevExt
	AddWall LeftPNL, ElevPanelMain, 0.2, -3.6, 2.5, -(3.667/2), 2.5, 7.25, 7.25, 0, 0, 0, 0
	DrawWalls = true, true, true, false, false, false
	TextureOverride %Prefix%ElevPanelMain, ElevExt, %Prefix%ElevPanelMain, ElevExt, ElevExt, ElevExt
	AddWall RightPNL, ElevPanelMain, 0.2, (3.667/2), 2.5, 3.6, 2.5, 7.25, 7.25, 0, 0, 0, 0
	DrawWalls = true, true, false, false, false, true
	TextureOverride %Prefix%ElevPanelTop, ElevExt, ElevExt, ElevExt, ElevExt, %Prefix%ElevPanelTop
	AddWall TopPNL, ElevPanelTop, 0.2, -(3.667/2), 2.5, (3.667/2), 2.5, 0.25, 0.25, 7, 7, 0, 0

	#Elevator number sign
	DrawWalls = true, false, false, false, false, false
	AddWall ElevNo_Inside, IN_ElevNo%elevator%, 0, 2.4835 - (0.45/2), 2.3 - 0.051, 2.4835 + (0.45/2), 2.3 - 0.051, 0.45, 0.45, 5.54, 5.54, 0, 0

	#Car Doors
	Set ElevDoorTex = WestElevDoors

	WallOrientation = left
	AddDoorComponent 1, LeftDR, %ElevDoorTex%, %ElevDoorTex%, 0.2, Left, 0.6, -(3.667/2), 2.5, 0, 2.5, 7, 0, 0, 0, 0, 0
	AddDoorComponent 1, RightDR, %ElevDoorTex%2, %ElevDoorTex%2, 0.2, Right, 0.6, 0, 2.5, (3.667/2), 2.5, 7, 0, 0, 0, 0, 0
	FinishDoors 1, false, false

		#Door Track
		FloorOrientation = top
		DrawWalls = true, true, true, true, true, true
		TextureOverride ElevExt, ElevDoorTrackZ, ElevExt, ElevExt, ElevExt, ElevExt
		AddFloor DoorTrack, ElevDoorTrackZ, 0.05, -3.667, 2.5, 3.667, 2.7, 0, 0, 0, 0

	#Button Panels
	Set Position = (3.667/2) + 0.65

	WallOrientation = right
	DrawWalls = true, false, true, true, true, true
	AddWall PanelBack, ElevPanel, 0.05, %Position% - 0.4, 2.3, %Position% + 0.4, 2.3, 4.55 + 0.1, 4.55 + 0.1, 2.2, 2.2, 0, 0

	CreatePanel Blank, 10, 3, front, %Position%, 2.3 - 0.051, 0.15, 0.15, 0.22, 0.25, 4, 0, 0

		#Buttons
		BtnPanelE1-4(1)
		
	#Floor Indicator
	Set IndOffset = 6.1 - 0.2

	DrawWalls = true, false, false, false, false, false
	AddWall IndicatorBack, IndBack, 0, %Position% - 0.3, 2.3 - 0.051, %Position% + 0.3, 2.3 - 0.051, 0.75 + 0.2, 0.75 + 0.2, %IndOffset%, %IndOffset%, 0, 0
	AddDirectionalIndicator true, true, true, Ind_Background, ArrowUnlit, ArrowUpLit, ArrowUnlit, ArrowDownLit, %Position% - 0.125, 2.3 - 0.052, %IndOffset% + 0.4 + 0.05, front, 0.25, 0.25, true, 0, 0
	AddFloorIndicator Ind_Button, front, %Position% + 0.125, 2.3 - 0.052, 0.25, 0.25, %IndOffset% + 0.4 + 0.05

	DrawWalls = true, true, true, true, true, true
	#Ropes
	RopePosition = 0, 7.25 + 0.2, 0
	RopeTexture = Ropes

	#Counterweight
	CreateCounterweight CWeight_Frame, CWeight, 0, -2.5 - 0.5, 2, 6, 0.5, 0.5

	#Rails
	AddRails ElevRail, ElevRailSide, 0, 0, false, 3.6 + 0.06, 0.1
	AddRails ElevRail, ElevRailSide, 0, -2.5 - 0.5, false, 1 + 0.06, 0.1
<EndElevators>

#Set textures for hall stations
<Function ElevHallTex>
	#Shaft doors
	Set DoorTexture = EastShaftElevDoors
	Set WestDoorTexture = WestShaftElevDoors

	#Call button panels
	Set BPanelTexture = ElevExtPanels

	#Lantern panels
	Set LPanelTexture = %BPanelTexture%
<EndFunction>

<Floors 0 to 5>
	#Shaft Doors
	Set StartNo = 1
	ElevHallTex() #Set shaft door and panel textures

	WallOrientation = right
	AddShaftDoorComponent %StartNo%, 1, LeftShaftDoor, %DoorTexture%, %DoorTexture%, 0.2, Right, 0.6, -3.1, 0, -3.1, (3.667/2), 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo%, 1, RightShaftDoor, (%DoorTexture%)2, (%DoorTexture%)2, 0.2, Left, 0.6, -3.1, -(3.667/2), -3.1, 0, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo%, 1, false, false

	WallOrientation = right
	AddShaftDoorComponent %StartNo% + 1, 1, LeftShaftDoor, %DoorTexture%, %DoorTexture%, 0.2, Right, 0.6, -3.1, 0, -3.1, (3.667/2), 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo% + 1, 1, RightShaftDoor, (%DoorTexture%)2, (%DoorTexture%)2, 0.2, Left, 0.6, -3.1, -(3.667/2), -3.1, 0, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo% + 1, 1, false, false

	Set StartNo = 3
	WallOrientation = left
	AddShaftDoorComponent %StartNo%, 1, LeftShaftDoor, %WestDoorTexture%, %WestDoorTexture%, 0.2, Right, 0.6, 0, 2.8, (3.667/2), 2.8, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo%, 1, RightShaftDoor, (%WestDoorTexture%)2, (%WestDoorTexture%)2, 0.2, Left, 0.6, -(3.667/2), 2.8, 0, 2.8, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo%, 1, false, false

	WallOrientation = left
	AddShaftDoorComponent %StartNo% + 1, 1, LeftShaftDoor, %WestDoorTexture%, %WestDoorTexture%, 0.2, Right, 0.6, 0, 2.8, (3.667/2), 2.8, 7, 0, 0, 0, 0, 0
	AddShaftDoorComponent %StartNo% + 1, 1, RightShaftDoor, (%WestDoorTexture%)2, (%WestDoorTexture%)2, 0.2, Left, 0.6, -(3.667/2), 2.8, 0, 2.8, 7, 0, 0, 0, 0, 0
	FinishShaftDoor %StartNo% + 1, 1, false, false

		#Door fillers
		#Elevators 1-2
		WallTex() #Set filler textures to match with shaft walls
		WallOrientation = left
		AddFillerWalls %6E%, 0.66, 71, -2.5, 3.667, 7, %interfloorheight%, false, 0, 0, true
		AddFillerWalls %6E%, 0.66, 71, -12.5, 3.667, 7, %interfloorheight%, false, 0, 0, true

		#Elevators 3-4
		Set 6 = ExtFacade2A #Override
		WallOrientation = right
		AddFillerWalls %6%, 0.66, -38.5, -130.5, 3.667, 7, %interfloorheight%, true, 0, 0, true
		AddFillerWalls %6%, 0.66, -48.5, -130.5, 3.667, 7, %interfloorheight%, true, 0, 0, true

		#Door tracks
		AddDoorTrack(1, 1, -4.17 + 0.33, -4.17 + 0.33 + 0.2, 5 - 3.667, 5 + 3.667, ElevDoorTrackX)
		AddDoorTrack(2, 1, -4.17 + 0.33, -4.17 + 0.33 + 0.2, -5 - 3.667, -5 + 3.667, ElevDoorTrackX)
		AddDoorTrack(3, 2, 5 - 3.667, 5 + 3.667, 4.17 - 0.33 - 0.2, 4.17 - 0.33, ElevDoorTrackZ)
		AddDoorTrack(4, 2, -5 - 3.667, -5 + 3.667, 4.17 - 0.33 - 0.2, 4.17 - 0.33, ElevDoorTrackZ)

	#Call buttons
		#Button & indicator panel
		WallOrientation = right
		DrawWalls = true, false, true, true, true, true
		AddWall E1-2ExtPanel, %BPanelTexture%, 0.05, 71, -7.5 - 0.4, 71, -7.5 + 0.4, 2.1, 2.1, 3.5, 3.5, 0, 0, false
		WallOrientation = left
		DrawWalls = false, true, true, true, true, true
		AddWall E3-4ExtPanel, %BPanelTexture%, 0.05, -43.5 - 0.4, -130.5, -43.5 + 0.4, -130.5, 2.1, 2.1, 3.5, 3.5, 0, 0, false

		#Buttons
		CallButtonElevators = 1, 2
		CreateCallButtons %BPanelTexture%, ButtonUp, ButtonUpLit, ButtonDown, ButtonDownLit, 71 - 0.051, -7.5, 4 - 0.4, left, 0.4, 0.8, false, 0, 0
		CallButtonElevators = 3, 4
		CreateCallButtons %BPanelTexture%, ButtonUp, ButtonUpLit, ButtonDown, ButtonDownLit, -43.5, -130.5 + 0.051, 4 - 0.4, back, 0.4, 0.8, false, 0, 0

		#Signs
		Set Suffix = %floorid%
		DrawWalls = true, false, false, false, false, false
		AddWall E1-2FloorSign, PG_FlrSign%Suffix%, 0, 71 - 0.001, -7.5 - 0.5, 71 - 0.001, -7.5 + 0.5, 1, 1, 6, 6, 0, 0, false
		#AddWall E1-2FloorSign, PG_FlrSign%Suffix%, 0, 71 - 0.001, -7.5 - 0.5, 71 - 0.001, -7.5 + 0.5, 1, 1, 5.5, 5.5, 0, 0, false
		AddWall E1-2HallSign, ElevHallSign, 0, 71 - 0.001, -7.5 - 0.375, 71 - 0.001, -7.5 + 0.375, 1, 1, 2.4, 2.4, 0, 0, false
		DrawWalls = false, true, false, false, false, false
		AddWall E3-4FloorSign, PG_FlrSign%Suffix%, 0, -43.5 - 0.5, -130.5 + 0.001, -43.5 + 0.5, -130.5 + 0.001, 1, 1, 6 + %interfloorheight%, 6 + %interfloorheight%, 0, 0, true
		AddWall E3-4HallSign, ElevHallSign, 0, -43.5 - 0.375, -130.5 + 0.001, -43.5 + 0.375, -130.5 + 0.001, 1, 1, 2.4 + %interfloorheight%, 2.4 + %interfloorheight%, 0, 0, true

	#Lanterns
		#AddDirectionalIndicator 1, true, false, false, true, %LPanelTexture%, LanternUp, LanternUpLit, LanternDown, LanternDownLit, -3.3 - 0.66 - 0.001, -(3.667/2) - 1.25, 5.5, left, 0.5, 1, true, 0, 0
		#AddDirectionalIndicator 2, true, false, false, true, %LPanelTexture%, LanternUp, LanternUpLit, LanternDown, LanternDownLit, -3.3 - 0.66 - 0.001, (3.667/2) + 1.25, 5.5, left, 0.5, 1, true, 0, 0
		#AddDirectionalIndicator 3, true, false, false, true, %LPanelTexture%, LanternUp, LanternUpLit, LanternDown, LanternDownLit, -(3.667/2) - 1.25, 3 + 0.66 + 0.001, 5.5, back, 0.5, 1, true, 0, 0
		#AddDirectionalIndicator 4, true, false, false, true, %LPanelTexture%, LanternUp, LanternUpLit, LanternDown, LanternDownLit, (3.667/2) + 1.25, 3 + 0.66 + 0.001, 5.5, back, 0.5, 1, true, 0, 0

	#Indicator sets
		#Elevators 1-2 (East)
		DrawWalls = true, false, false, false, false, false
		AddWall IndicatorBack, IndBack, 0, 71 - 0.051, -7.5 - 0.3, 71 - 0.051, -7.5 + 0.3, 0.9, 0.9, 4.5 + 0.02, 4.5 + 0.02, 0, 0, false
		AddFloorIndicator 1, false, Ind2_Button, left, 71 - 0.052, -7.5 + 0.15, 0.25, 0.25, 4.8 - 0.125 + 0.1
		AddDirectionalIndicator 1, false, true, true, true, Black, ArrowUnlit, ArrowUpLit, ArrowUnlit, ArrowDownLit, 71 - 0.052, -7.5 + 0.15, 5.1 - 0.125 + 0.05, left, 0.25, 0.25, false, 0, 0

		AddFloorIndicator 2, false, Ind2_Button, left, 71 - 0.052, -7.5 - 0.15, 0.25, 0.25, 4.8 - 0.125 + 0.1
		AddDirectionalIndicator 2, false, true, true, true, Black, ArrowUnlit, ArrowUpLit, ArrowUnlit, ArrowDownLit, 71 - 0.052, -7.5 - 0.15, 5.1 - 0.125 + 0.05, left, 0.25, 0.25, false, 0, 0

		#Elevators 3-4 (West)
		DrawWalls = false, true, false, false, false, false
		AddWall IndicatorBack, IndBack, 0, -43.5 - 0.3, -130.5 + 0.051, -43.5 + 0.3, -130.5 + 0.051, 0.9, 0.9, 4.5 + 0.02, 4.5 + 0.02, 0, 0, false
		AddFloorIndicator 3, false, Ind2_Button, back, -43.5 + 0.15, -130.5 + 0.052, 0.25, 0.25, 4.8 - 0.125 + 0.1
		AddDirectionalIndicator 3, false, true, true, true, Black, ArrowUnlit, ArrowUpLit, ArrowUnlit, ArrowDownLit, -43.5 + 0.15, -130.5 + 0.052, 5.1 - 0.125 + 0.05, back, 0.25, 0.25, false, 0, 0

		AddFloorIndicator 4, false, Ind2_Button, back, -43.5 - 0.15, -130.5 + 0.052, 0.25, 0.25, 4.8 - 0.125 + 0.1
		AddDirectionalIndicator 4, false, true, true, true, Black, ArrowUnlit, ArrowUpLit, ArrowUnlit, ArrowDownLit, -43.5 - 0.15, -130.5 + 0.052, 5.1 - 0.125 + 0.05, back, 0.25, 0.25, false, 0, 0
